<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Kids Movies</name>
    <match>one</match>
    <rule field="path" operator="contains">
        <value>/kids/</value>
    </rule>
    <rule field="path" operator="contains">
        <value>/kids movies/</value>
    </rule>
    <order direction="descending">lastplayed</order>
</smartplaylist>
