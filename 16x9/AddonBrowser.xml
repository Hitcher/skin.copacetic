<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- addonbrowser -->
	<onload condition="!String.Contains(Control.GetLabel(7),*) | String.IsEmpty(Skin.String(AddonForeign))">Skin.SetString(AddonForeign,off)</onload>
	<onload condition="!String.Contains(Control.GetLabel(8),*) | String.IsEmpty(Skin.String(AddonBroken))">Skin.SetString(AddonBroken,off)</onload>
	<include>My_Window_Fade_Animation</include>
	<defaultcontrol always="true">50</defaultcontrol>
	<menucontrol>3000</menucontrol>
	<views>50,505</views>
	<controls>

		<include content="MyViews_Hidden_Controls"/>
		<include condition="Skin.HasSetting(Multiart_Slideshow)" content="MyViews_Multiart_Labels"/>
		<include content="MyViews_Main"/>
		<include content="MyViews_Scrollbars"/>
		<include content="MyViews_Options_Menu"/>

	</controls>
</window>
