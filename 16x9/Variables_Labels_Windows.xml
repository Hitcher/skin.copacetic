<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<variable name="loginscreen_Welcome_Label">
		<!-- Good morning / afternoon, / evening... Who's watching?... Seasonal messages -->
		<value condition="System.Time(17:00,00:00) + Integer.IsGreaterOrEqual(System.Time(hh),5)">$LOCALIZE[31101] $LOCALIZE[31100]</value>
		<value condition="System.Time(12:00,17:00)">$LOCALIZE[31102] $LOCALIZE[31100]</value>
		<value>$LOCALIZE[31103] $LOCALIZE[31100]</value>
	</variable>

	<variable name="musicplaylisteditor_Override_LeftLabel">
		<value condition="String.IsEqual(ListItem.DBType,song)">$VAR[TrackNumber_Label,,.]$INFO[ListItem.Title, ]</value>
		<value>$INFO[ListItem.Label]</value>
	</variable>

	<variable name="musicplaylisteditor_Override_LeftLabel2">
		<value condition="String.IsEqual(ListItem.DBType,song)">$INFO[ListItem.Label2]</value>
		<value/>
	</variable>

	<variable name="musicplaylisteditor_Directory_Label">
		<!-- items / item / No items -->
		<value condition="Integer.IsGreater(Container(50).NumItems,1)">$INFO[Container(50).NumItems,, $LOCALIZE[31001]]</value>
		<value condition="Integer.IsGreater(Container(50).NumItems,0)">$INFO[Container(50).NumItems,, $LOCALIZE[31000]]</value>
		<value>$LOCALIZE[31136]</value>
	</variable>

	<variable name="musicplaylisteditor_Info_Label">
		<value condition="!Control.HasFocus(50) + !Control.HasFocus(60) + Integer.IsGreater(Container(100).NumItems,1)">$INFO[Container(100).ListItem.Artist][CR]$INFO[Container(100).ListItem.Album][CR]$INFO[Container(100).ListItem.Year]</value>
		<value condition="[Control.HasFocus(50) | Control.HasFocus(60)] + String.IsEqual(Container(50).ListItem.DBType,song)">$INFO[Container(50).ListItem.Artist][CR]$INFO[Container(50).ListItem.Artist][CR]$INFO[Container(50).ListItem.Year]</value>
		<value condition="[Control.HasFocus(50) | Control.HasFocus(60)] + String.IsEqual(Container(50).ListItem.DBType,album)">$INFO[Container(50).ListItem.Artist][CR]$INFO[Container(50).ListItem.Year]</value>
	</variable>

	<variable name="musicplaylisteditor_Playlist_Label">
		<!-- songs / song / No songs added -->
		<value condition="Integer.IsGreater(Container(100).NumItems,1)">$INFO[Container(100).NumItems,, $LOCALIZE[31137]]</value>
		<value condition="Integer.IsGreater(Container(100).NumItems,0)">$INFO[Container(100).NumItems,, $LOCALIZE[31140]]</value>
		<value>$LOCALIZE[31139]</value>
	</variable>

	<variable name="script-upnext_Label">
		<!-- Game of Thrones / 2x02 / The Night Lands -->
		<value condition="!Integer.IsGreater(Window.Property(Episode),9)">$INFO[Window.Property(tvshowtitle)]$INFO[Window.Property(season), $LOCALIZE[31116] ]$INFO[Window.Property(episode),x0]$INFO[Window.Property(title), $LOCALIZE[31116] ]</value>
		<value condition="!Integer.IsGreater(Window.Property(Episode),9)">$INFO[Window.Property(tvshowtitle)]$INFO[Window.Property(season), $LOCALIZE[31116] ]$INFO[Window.Property(episode),x]$INFO[Window.Property(title), $LOCALIZE[31116] ]</value>
	</variable>

	<variable name="skinsettings_Choose_Widget1_Path_Label">
		<value condition="!String.IsEmpty(Skin.String(Widget1_Custom_Path))">$INFO[Skin.String(Widget1_Custom_Path),- ]</value>
		<value>- $LOCALIZE[573]</value>
	</variable>

	<variable name="skinsettings_Choose_Widget2_Path_Label">
