	<variable name="skinshortcuts_Background_Override_Label2">
		<value condition="!String.IsEmpty(Container(211).ListItem.Property(backgroundPlaylist))">$INFO[Container(211).ListItem.Property(backgroundPlaylist)]</value>
		<value condition="!String.IsEmpty(Container(211).ListItem.Property(background))">$INFO[Container(211).ListItem.Property(background)]</value>
		<value/>
	</variable>

	<variable name="subtitlesearch_Label">
		<value condition="ListItem.Property(hearingimp) + ListItem.Property(sync)">$INFO[ListItem.Label,, CC SYNC]$INFO[ListItem.Label2, $LOCALIZE[31116] ]</value>
		<value condition="ListItem.Property(hearingimp)">$INFO[ListItem.Label,, CC]$INFO[ListItem.Label2, $LOCALIZE[31116] ]</value>
		<value condition="ListItem.Property(sync)">$INFO[ListItem.Label,, SYNC]$INFO[ListItem.Label2, $LOCALIZE[31116] ]</value>
		<value>$INFO[ListItem.Label]$INFO[ListItem.Label2, $LOCALIZE[31116] ]</value>
	</variable>

	<variable name="volumebar_Info_Label">
		<!-- Passthrough audio in use / Muted / 33% -->
		<value condition="Player.Passthrough">$LOCALIZE[29802]</value>
		<value condition="Player.Muted">$LOCALIZE[31144]</value>
		<value>$INFO[Control.GetLabel(50660),,%]</value>
	</variable>

</includes>
