<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<variable name="loginscreen_Welcome_Label">
		<!-- Good morning / afternoon, / evening / Seasonal messages... Who's watching? -->
		<value condition="System.Date(12-25,12-26)">$LOCALIZE[31711]</value>
		<value condition="System.Date(04-22,04-23)">$LOCALIZE[31714]</value>
		<value condition="System.Date(04-21,04-22) | System.Date(06-28,06-29)">$LOCALIZE[31713]</value>
		<value condition="System.Date(03-12,03-13)">$LOCALIZE[31710]</value>
		<value condition="System.Date(03-11,03-12)">$LOCALIZE[31709]</value>
		<value condition="System.Date(03-22,03-23)">$LOCALIZE[31712]</value>
		<value condition="System.Date(03-08,03-09)">$LOCALIZE[31708][CR]</value>
		<value condition="System.Date(02-19,02-20)">$LOCALIZE[31707]</value>
		<value condition="System.Date(02-11,02-12)">$LOCALIZE[31706]</value>
		<value condition="System.Date(01-19,01-20)">$LOCALIZE[31705][CR]</value>
		<value condition="System.Date(01-01,01-02)">$LOCALIZE[31704]</value>
		<value condition="System.Time(17:00,00:00) + Integer.IsGreaterOrEqual(System.Time(hh),5)">$LOCALIZE[31701]</value>
		<value condition="System.Time(12:00,17:00)">$LOCALIZE[31702]</value>
		<value>$LOCALIZE[31703]</value>
	</variable>

	<variable name="musicplaylisteditor_Override_LeftLabel">
		<value condition="String.IsEqual(ListItem.DBType,song)">$VAR[TrackNumber_Label,,.]$INFO[ListItem.Title, ]</value>
		<value>$INFO[ListItem.Label]</value>
	</variable>

	<variable name="musicplaylisteditor_Override_LeftLabel2">
		<value condition="String.IsEqual(ListItem.DBType,song)">$INFO[ListItem.Label2]</value>
		<value/>
	</variable>

	<variable name="musicplaylisteditor_Directory_Label">
		<!-- items / item / No items -->
		<value condition="Integer.IsGreater(Container(50).NumItems,1)">$INFO[Container(50).NumItems,, $LOCALIZE[31522]]</value>
		<value condition="Integer.IsGreater(Container(50).NumItems,0)">$INFO[Container(50).NumItems,, $LOCALIZE[31523]]</value>
		<value>$LOCALIZE[31525]</value>
	</variable>

	<variable name="musicplaylisteditor_Info_Label">
		<value condition="!Control.HasFocus(50) + !Control.HasFocus(60) + Integer.IsGreater(Container(100).NumItems,1)">$INFO[Container(100).ListItem.Artist][CR]$INFO[Container(100).ListItem.Album][CR]$INFO[Container(100).ListItem.Year]</value>
		<value condition="[Control.HasFocus(50) | Control.HasFocus(60)] + String.IsEqual(Container(50).ListItem.DBType,song)">$INFO[Container(50).ListItem.Artist][CR]$INFO[Container(50).ListItem.Artist][CR]$INFO[Container(50).ListItem.Year]</value>
		<value condition="[Control.HasFocus(50) | Control.HasFocus(60)] + String.IsEqual(Container(50).ListItem.DBType,album)">$INFO[Container(50).ListItem.Artist][CR]$INFO[Container(50).ListItem.Year]</value>
	</variable>

	<variable name="musicplaylisteditor_Playlist_Label">
		<!-- songs / song / No songs added -->
		<value condition="Integer.IsGreater(Container(100).NumItems,1)">$INFO[Container(100).NumItems,, $LOCALIZE[31526]]</value>
		<value condition="Integer.IsGreater(Container(100).NumItems,0)">$INFO[Container(100).NumItems,, $LOCALIZE[31527]]</value>
		<value>$LOCALIZE[31528]</value>
	</variable>

	<variable name="script-upnext_Label">
		<!-- Game of Thrones / 2x02 / The Night Lands -->
		<value condition="!Integer.IsGreater(Window.Property(Episode),9)">$INFO[Window.Property(tvshowtitle)]$INFO[Window.Property(season), $LOCALIZE[31001] ]$INFO[Window.Property(episode),x0]$INFO[Window.Property(title), $LOCALIZE[31001] ]</value>
		<value condition="!Integer.IsGreater(Window.Property(Episode),9)">$INFO[Window.Property(tvshowtitle)]$INFO[Window.Property(season), $LOCALIZE[31001] ]$INFO[Window.Property(episode),x]$INFO[Window.Property(title), $LOCALIZE[31001] ]</value>
	</variable>

