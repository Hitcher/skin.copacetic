			<animation effect="fade" start="0" end="100" time="360" tween="sine" easing="out" reversible="false" condition="Control.HasFocus(90$PARAM[widget_position]0)">Conditional</animation>
			<include condition="Skin.HasSetting(Widget$PARAM[widget_position]_View_Billboard)" content="BillboardView_Display">
