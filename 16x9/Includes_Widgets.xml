<includes>

	<include name="Container_Content_Path">
		<definition>
			<content target="$PARAM[target]" sortby="$PARAM[sortby]" sortorder="$PARAM[sortorder]" limit="20">$PARAM[content]</content>
		</definition>
	</include>

	<include name="Widget_List">
		<param name="id"/>
		<param name="use_custom_content"/>
		<param name="use_custom_click" default="false"/>
		<param name="click_action"/>
		<definition>
			<control type="fixedlist" id="$PARAM[id]">
				<visible>Integer.IsGreater(Container($PARAM[id]).NumItems,0) | Container($PARAM[id]).IsUpdating</visible>
				<onfocus>SetProperty(HomeWidget,$PARAM[id],home)</onfocus>
				<onunfocus condition="!String.IsEmpty(window(home).property(HomeWidget))">ClearProperty(HomeWidget,home)</onunfocus>
				<viewtype label="widgetlist">list</viewtype>
				<preloaditems>2</preloaditems>
				<orientation>horizontal</orientation>
				<scrolltime tween="sine" easing="inout">360</scrolltime>
				<include condition="$PARAM[use_custom_click]">ClickAction_$PARAM[click_action]</include>
				<onleft>0</onleft>
				<onright>0</onright>
				<onup condition="Control.HasFocus(8010)">SetFocus(65000)</onup>
				<onup condition="Control.HasFocus(8020)">SetFocus(65000)</onup>
				<onup condition="Control.HasFocus(8030) + Control.IsVisible(8020)">8020</onup>
				<onup condition="Control.HasFocus(8030) + !Control.IsVisible(8020)">SetFocus(65000)</onup>
				<onup condition="Control.HasFocus(9010)">9000</onup>
				<onup condition="Control.HasFocus(9020)">9010</onup>
				<onup condition="Control.HasFocus(9030)">9020</onup>
				<onup condition="Control.HasFocus(9040)">9030</onup>
				<onup condition="Control.HasFocus(9050)">9040</onup>
				<onup condition="Control.HasFocus(9060)">9050</onup>
				<onup condition="Control.HasFocus(9070)">9060</onup>
				<onup condition="Control.HasFocus(9080)">9070</onup>
				<onup condition="Control.HasFocus(9090)">9080</onup>
				<ondown condition="Control.HasFocus(8010)">SetFocus(65000)</ondown>
				<ondown condition="Control.HasFocus(8020) + Control.IsVisible(8030)">8030</ondown>
				<ondown condition="Control.HasFocus(8020) + !Control.IsVisible(8030)">SetFocus(65000)</ondown>
				<ondown condition="Control.HasFocus(8030)">SetFocus(65000)</ondown>
				<ondown condition="Control.HasFocus(9010) + Control.IsVisible(9020)">9020</ondown>
				<ondown condition="Control.HasFocus(9010) + !Control.IsVisible(9020)">9000</ondown>
				<ondown condition="Control.HasFocus(9020) + Control.IsVisible(9030)">9030</ondown>
				<ondown condition="Control.HasFocus(9020) + !Control.IsVisible(9030)">9000</ondown>
				<ondown condition="Control.HasFocus(9030) + Control.IsVisible(9040)">9040</ondown>
				<ondown condition="Control.HasFocus(9030) + !Control.IsVisible(9040)">9000</ondown>
				<ondown condition="Control.HasFocus(9040) + Control.IsVisible(9050)">9050</ondown>
				<ondown condition="Control.HasFocus(9040) + !Control.IsVisible(9050)">9000</ondown>
				<ondown condition="Control.HasFocus(9050) + Control.IsVisible(9060)">9060</ondown>
				<ondown condition="Control.HasFocus(9050) + !Control.IsVisible(9060)">9000</ondown>
				<ondown condition="Control.HasFocus(9060) + Control.IsVisible(9070)">9070</ondown>
				<ondown condition="Control.HasFocus(9060) + !Control.IsVisible(9070)">9000</ondown>
				<ondown condition="Control.HasFocus(9070) + Control.IsVisible(9080)">9080</ondown>
				<ondown condition="Control.HasFocus(9070) + !Control.IsVisible(9080)">9000</ondown>
				<ondown condition="Control.HasFocus(9080) + Control.IsVisible(9090)">9090</ondown>
				<ondown condition="Control.HasFocus(9080) + !Control.IsVisible(9090)">9000</ondown>
				<ondown condition="Control.HasFocus(9090)">9000</ondown>
				<onback condition="Window.IsVisible(home)">9000</onback>
				<onback condition="Window.IsVisible(movieinformation)">65005</onback>
				<focusedlayout>
					<include>HiddenWidgetFocusButton</include>
				</focusedlayout>
				<itemlayout/>
				<include condition="$PARAM[use_custom_content]" content="Container_Content_Path">
					<param name="target" value="$PARAM[target]"/>
					<param name="sortby" value="$PARAM[sortby]"/>
					<param name="sortorder" value="$PARAM[sortorder]"/>
					<param name="content" value="$PARAM[content]"/>
				</include>
			</control>
		</definition>
	</include>

