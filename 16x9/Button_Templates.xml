<includes>

	<include name="Simple_Button_Template">
		<description>$PARAM[description]</description>
		<width>60</width>
		<height>60</height>
		<font></font>
		<texturefocus colordiffuse="$VAR[Light_Color]">osd/icon_$PARAM[icon]_focused.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[Light_Color]">osd/icon_$PARAM[icon]_unfocused.png</texturenofocus>
	</include>

	<include name="Music_Player_Button_Template">
		<param name="visible" default="true"/>
		<param name="focused"/>
		<param name="icon"/>
		<param name="left"/>
		<param name="top"/>
		<definition>
			<control type="group">
				<visible>$PARAM[visible]</visible>
				<left>$PARAM[left]</left>
				<top>$PARAM[top]</top>
				<width>60</width>
				<height>60</height>
				<control type="image">
					<visible>!$PARAM[focused]</visible>
					<texture colordiffuse="$VAR[Volume_Focus_Color]">osd/icon_$PARAM[icon]_unfocused.png</texture>
				</control>
				<control type="group">
					<visible>$PARAM[focused]</visible>
					<control type="image">
						<texture colordiffuse="$VAR[Volume_Focus_Color]">osd/icon_$PARAM[icon]_focused.png</texture>
					</control>
				</control>
			</control>
		</definition>
	</include>

	<include name="Button_Template">
		<param name="buttonid" default=""/>
		<param name="left" default="0"/>
		<param name="width" default="120"/>
		<param name="unfocusedcolor" default="$VAR[List_Text_Color]"/>
		<param name="focusedcolor" default="$VAR[List_Text_Color]"/>
		<param name="texture" default="Cross"/>
		<param name="border" default="false"/>
		<definition>
			<control type="group">
				<visible>[!Window.IsVisible(selectdialog) + !String.IsEqual(Window.Property(xmlfile),DialogSettings.xml) + Control.IsEnabled($PARAM[buttonid]) + Control.IsVisible($PARAM[buttonid])] | [Window.IsVisible(selectdialog) + Control.IsVisible($PARAM[buttonid])] | [String.IsEqual(Window.Property(xmlfile),DialogSettings.xml) + !String.IsEmpty(Control.GetLabel($PARAM[buttonid]))]</visible>
				<left>$PARAM[left]</left>
				<width>$PARAM[width]</width>
				<height>$PARAM[width]</height>
				<control type="group">
					<centertop>50%</centertop>
					<centerleft>50%</centerleft>
					<visible>!Control.HasFocus($PARAM[buttonid])</visible>
					<include content="$PARAM[texture]_unfocused">
						<param name="width" value="$PARAM[width]"/>
						<param name="color" value="$PARAM[unfocusedcolor]"/>
					</include>
				</control>
				<control type="group">
					<centertop>50%</centertop>
					<centerleft>50%</centerleft>
					<visible>Control.HasFocus($PARAM[buttonid])</visible>
					<include content="$PARAM[texture]_focused">
						<param name="width" value="$PARAM[width]"/>
						<param name="color" value="$PARAM[focusedcolor]"/>
					</include>
					<control type="image">
						<visible>$PARAM[border]</visible>
						<bottom>15</bottom>
						<left>30</left>
						<right>30</right>
						<height>5</height>
						<texture colordiffuse="$VAR[Accent_Color]">views/white_100.png</texture>
					</control>
				</control>
			</control>
		</definition>
	</include>

	<include name="Button_Template_Special">
		<param name="buttonid" default=""/>
		<param name="left" default="0"/>
		<param name="width" default="120"/>
		<param name="unfocusedcolor" default="$VAR[List_Text_Color]"/>
		<param name="focusedcolor" default="$VAR[List_Text_Color]"/>
		<param name="texture" default="Cross"/>
		<param name="border" default="false"/>
		<definition>
			<control type="group">
				<visible>[!Window.IsVisible(selectdialog) + !String.IsEqual(Window.Property(xmlfile),DialogSettings.xml) + Control.IsEnabled($PARAM[buttonid]) + Control.IsVisible($PARAM[buttonid])] | [Window.IsVisible(selectdialog) + Control.IsVisible($PARAM[buttonid])] | [String.IsEqual(Window.Property(xmlfile),DialogSettings.xml) + !String.IsEmpty(Control.GetLabel($PARAM[buttonid]))]</visible>
				<left>$PARAM[left]</left>
				<width>$PARAM[width]</width>
				<height>$PARAM[width]</height>
				<control type="group">
					<centertop>50%</centertop>
					<centerleft>50%</centerleft>
					<visible>!Control.HasFocus($PARAM[buttonid]) + !String.Contains(Control.GetLabel($PARAM[buttonid]),*)</visible>
					<include content="$PARAM[texture]_unfocused">
						<param name="width" value="$PARAM[width]"/>
						<param name="color" value="$PARAM[unfocusedcolor]"/>
					</include>
				</control>
				<control type="group">
					<centertop>50%</centertop>
					<centerleft>50%</centerleft>
					<control type="group">
						<visible>Control.HasFocus($PARAM[buttonid]) | String.Contains(Control.GetLabel($PARAM[buttonid]),*)</visible>
						<include content="$PARAM[texture]_focused">
							<param name="width" value="$PARAM[width]"/>
							<param name="color" value="$PARAM[focusedcolor]"/>
						</include>
					</control>
					<control type="image">
						<visible>String.Contains(Control.GetLabel($PARAM[buttonid]),*)</visible>
						<visible>$PARAM[border]</visible>
						<bottom>15</bottom>
						<left>30</left>
						<right>30</right>
						<height>5</height>
						<texture colordiffuse="$VAR[Accent_Color]">views/white_100.png</texture>
					</control>
				</control>
			</control>
		</definition>
	</include>

	<include name="Button_Template_Toggle">
		<param name="buttonid" default=""/>
		<param name="left" default="0"/>
		<param name="width" default="120"/>
		<param name="unfocusedcolor" default="$VAR[List_Text_Color]"/>
		<param name="focusedcolor" default="$VAR[List_Text_Color]"/>
		<param name="texture" default="Cross"/>
		<param name="alttexture" default="Plus"/>
		<param name="usealttexture" default="false"/>
		<param name="border" default="false"/>
		<definition>
			<control type="group">
				<visible>[!Window.IsVisible(selectdialog) + !String.IsEqual(Window.Property(xmlfile),DialogSettings.xml) + Control.IsEnabled($PARAM[buttonid]) + Control.IsVisible($PARAM[buttonid])] | [Window.IsVisible(selectdialog) + Control.IsVisible($PARAM[buttonid])] | [String.IsEqual(Window.Property(xmlfile),DialogSettings.xml) + !String.IsEmpty(Control.GetLabel($PARAM[buttonid]))]</visible>
				<left>$PARAM[left]</left>
				<width>$PARAM[width]</width>
				<height>$PARAM[width]</height>
				<control type="group">
					<visible>!$PARAM[usealttexture]</visible>
					<control type="group">
						<visible>!Control.HasFocus($PARAM[buttonid])</visible>
						<centertop>50%</centertop>
						<centerleft>50%</centerleft>
						<include content="$PARAM[texture]_unfocused">
							<param name="width" value="$PARAM[width]"/>
							<param name="color" value="$PARAM[unfocusedcolor]"/>
						</include>
					</control>
					<control type="group">
						<centertop>50%</centertop>
						<centerleft>50%</centerleft>
						<control type="group">
							<visible>Control.HasFocus($PARAM[buttonid])</visible>
							<include content="$PARAM[texture]_focused">
								<param name="width" value="$PARAM[width]"/>
								<param name="color" value="$PARAM[focusedcolor]"/>
							</include>
						</control>
						<control type="image">
							<visible>Control.HasFocus($PARAM[buttonid])</visible>
							<visible>$PARAM[border]</visible>
							<bottom>15</bottom>
							<left>30</left>
							<right>30</right>
							<height>5</height>
							<texture colordiffuse="$VAR[Accent_Color]">views/white_100.png</texture>
						</control>
					</control>
				</control>
				<control type="group">
					<visible>$PARAM[usealttexture]</visible>
					<control type="group">
						<visible>!Control.HasFocus($PARAM[buttonid])</visible>
						<centertop>50%</centertop>
						<centerleft>50%</centerleft>
						<include content="$PARAM[alttexture]_unfocused">
							<param name="width" value="$PARAM[width]"/>
							<param name="color" value="$PARAM[unfocusedcolor]"/>
						</include>
					</control>
					<control type="group">
						<centertop>50%</centertop>
						<centerleft>50%</centerleft>
						<control type="group">
							<visible>Control.HasFocus($PARAM[buttonid])</visible>
							<include content="$PARAM[alttexture]_focused">
								<param name="width" value="$PARAM[width]"/>
								<param name="color" value="$PARAM[focusedcolor]"/>
							</include>
						</control>
						<control type="image">
							<visible>Control.HasFocus($PARAM[buttonid])</visible>
							<visible>$PARAM[border]</visible>
							<bottom>15</bottom>
							<left>30</left>
							<right>30</right>
							<height>5</height>
							<texture colordiffuse="$VAR[Accent_Color]">views/white_100.png</texture>
						</control>
					</control>
				</control>
			</control>
		</definition>
	</include>

	<include name="Label_Button_Template">
		<param name="buttonid" default=""/>
		<param name="color" default="$VAR[List_Text_Color]"/>
		<param name="unfocusedfont" default="Breadcrumb_Info_Label"/>
		<param name="focusedfont" default="Breadcrumb_Title_Label"/>
		<param name="unfocusedheight" default="36"/>
		<param name="unfocusedtop" default="16"/>
		<param name="align" default="left"/>
		<param name="aligny" default="center"/>
		<definition>
			<control type="label">
				<top>$PARAM[unfocusedtop]</top>
				<width>auto</width>
				<height>$PARAM[unfocusedheight]</height>
				<visible>!String.IsEmpty(Control.GetLabel($PARAM[buttonid])) + Control.IsEnabled($PARAM[buttonid]) + Control.IsVisible($PARAM[buttonid]) + !Control.HasFocus($PARAM[buttonid])</visible>
				<label>$INFO[Control.GetLabel($PARAM[buttonid])]</label>
				<font>$PARAM[unfocusedfont]</font>
				<textcolor>$PARAM[color]</textcolor>
				<align>$PARAM[align]</align>
				<aligny>$PARAM[aligny]</aligny>
			</control>
			<control type="label">
				<top>0</top>
				<height>100%</height>
				<width>auto</width>
				<visible>!String.IsEmpty(Control.GetLabel($PARAM[buttonid])) + Control.IsEnabled($PARAM[buttonid]) + Control.IsVisible($PARAM[buttonid]) + Control.HasFocus($PARAM[buttonid])</visible>
				<label>$INFO[Control.GetLabel($PARAM[buttonid])]</label>
				<font>$PARAM[focusedfont]</font>
				<textcolor>$PARAM[color]</textcolor>
				<align>$PARAM[align]</align>
				<aligny>$PARAM[aligny]</aligny>
			</control>
		</definition>
	</include>


	<include name="Button_Group_Template">
		<param name="visible" default="true"/>
		<param name="id" default="9000"/>
		<param name="right" default="60"/>
		<param name="bottom" default="30"/>
		<param name="onleft" default="noop"/>
		<param name="onright" default="noop"/>
		<param name="onup" default="noop"/>
		<param name="ondown" default="noop"/>
		<param name="align" default="right"/>
		<param name="color" default="$VAR[Overlay_Text_Color]"/>
		<!-- icons -->
		<param name="icons" default="String.IsEqual(Control.GetLabel(11),$LOCALIZE[186])"/>
		<param name="buttonwidth" default="120"/>
		<param name="buttonborder" default="false"/>

		<param name="button1" default="false"/>
		<param name="button1id" default="38501"/>
		<param name="button1onclick" default=""/>
		<param name="button1onclick2" default=""/>
		<param name="button1label" default=""/>
		<param name="button1left" default="0"/>
		<param name="button1texture" default="Cross"/>
		<param name="button1alttexture" default="Cross"/>
		<param name="button1usealttexture" default="false"/>

		<param name="button2" default="false"/>
		<param name="button2id" default="38502"/>
		<param name="button2onclick" default=""/>
		<param name="button2onclick2" default=""/>
		<param name="button2label" default=""/>
		<param name="button2left" default="0"/>
		<param name="button2texture" default="Check"/>
		<param name="button2alttexture" default="Cross"/>
		<param name="button2usealttexture" default="false"/>

		<param name="button3" default="false"/>
		<param name="button3id" default="38503"/>
		<param name="button3onclick" default=""/>
		<param name="button3onclick2" default=""/>
		<param name="button3label" default=""/>
		<param name="button3left" default="0"/>
		<param name="button3texture" default="Cross"/>
		<param name="button3alttexture" default="Cross"/>
		<param name="button3usealttexture" default="false"/>

		<param name="button4" default="false"/>
		<param name="button4id" default="38504"/>
		<param name="button4onclick" default=""/>
		<param name="button4onclick2" default=""/>
		<param name="button4label" default=""/>
		<param name="button4left" default="0"/>
		<param name="button4texture" default="Check"/>
		<param name="button4alttexture" default="Cross"/>
		<param name="button4usealttexture" default="false"/>
		<!-- labels -->
		<param name="labelgroupheight" default="60"/>
		<param name="focusedfont" default="Breadcrumb_Title_Label"/>
		<param name="unfocusedfont" default="Breadcrumb_Info_Label"/>
		<param name="unfocusedtop" default="16"/>
		<param name="unfocusedheight" default="36"/>
		<definition>
			<control type="group">
				<visible>$PARAM[visible]</visible>
				<control type="grouplist" id="$PARAM[id]">
					<onleft>$PARAM[onleft]</onleft>
					<onright>$PARAM[onright]</onright>
					<onup>$PARAM[onup]</onup>
					<ondown>$PARAM[ondown]</ondown>
					<orientation>horizontal</orientation>
					<control type="button" id="$PARAM[button1id]">
						<visible>$PARAM[button1]</visible>
						<onclick>$PARAM[button1onclick]</onclick>
						<onclick>$PARAM[button1onclick2]</onclick>
						<label>$PARAM[button1label]</label>
						<include>Hidden_Button</include>
					</control>
					<control type="button" id="$PARAM[button2id]">
						<visible>$PARAM[button2]</visible>
						<onclick>$PARAM[button2onclick]</onclick>
						<onclick>$PARAM[button2onclick2]</onclick>
						<label>$PARAM[button2label]</label>
						<include>Hidden_Button</include>
					</control>
					<control type="button" id="$PARAM[button3id]">
						<visible>$PARAM[button3]</visible>
						<onclick>$PARAM[button3onclick]</onclick>
						<onclick>$PARAM[button3onclick2]</onclick>
						<label>$PARAM[button3label]</label>
						<include>Hidden_Button</include>
					</control>
					<control type="button" id="$PARAM[button4id]">
						<visible>$PARAM[button4]</visible>
						<onclick>$PARAM[button4onclick]</onclick>
						<onclick>$PARAM[button4onclick2]</onclick>
						<label>$PARAM[button4label]</label>
						<include>Hidden_Button</include>
					</control>
				</control>
				<control type="grouplist">
					<visible>$PARAM[icons]</visible>
					<right>$PARAM[right]</right>
					<bottom>$PARAM[bottom]</bottom>
					<width>100%</width>
					<height>$PARAM[buttonwidth]</height>
					<itemgap>0</itemgap>
					<align>right</align>
					<orientation>horizontal</orientation>
					<usecontrolcoords>true</usecontrolcoords>
					<include content="Button_Template_Toggle">
						<param name="buttonid" value="$PARAM[button1id]"/>
						<param name="left" value="$PARAM[button1left]"/>
						<param name="width" value="$PARAM[buttonwidth]"/>
						<param name="texture" value="$PARAM[button1texture]"/>
						<param name="alttexture" value="$PARAM[button1alttexture]"/>
						<param name="usealttexture" value="$PARAM[button1usealttexture]"/>
						<param name="unfocusedcolor" value="$PARAM[color]"/>
						<param name="focusedcolor" value="$PARAM[color]"/>
						<param name="border" value="$PARAM[buttonborder]"/>
					</include>
					<include content="Button_Template_Toggle">
						<param name="buttonid" value="$PARAM[button2id]"/>
						<param name="left" value="$PARAM[button2left]"/>
						<param name="width" value="$PARAM[buttonwidth]"/>
						<param name="texture" value="$PARAM[button2texture]"/>
						<param name="alttexture" value="$PARAM[button2alttexture]"/>
						<param name="usealttexture" value="$PARAM[button2usealttexture]"/>
						<param name="unfocusedcolor" value="$PARAM[color]"/>
						<param name="focusedcolor" value="$PARAM[color]"/>
						<param name="border" value="$PARAM[buttonborder]"/>
					</include>
					<include content="Button_Template_Toggle">
						<param name="buttonid" value="$PARAM[button3id]"/>
						<param name="left" value="$PARAM[button3left]"/>
						<param name="width" value="$PARAM[buttonwidth]"/>
						<param name="texture" value="$PARAM[button3texture]"/>
						<param name="alttexture" value="$PARAM[button3alttexture]"/>
						<param name="usealttexture" value="$PARAM[button3usealttexture]"/>
						<param name="unfocusedcolor" value="$PARAM[color]"/>
						<param name="focusedcolor" value="$PARAM[color]"/>
						<param name="border" value="$PARAM[buttonborder]"/>
					</include>
					<include content="Button_Template_Toggle">
						<param name="buttonid" value="$PARAM[button4id]"/>
						<param name="left" value="$PARAM[button4left]"/>
						<param name="width" value="$PARAM[buttonwidth]"/>
						<param name="texture" value="$PARAM[button4texture]"/>
						<param name="alttexture" value="$PARAM[button4alttexture]"/>
						<param name="usealttexture" value="$PARAM[button4usealttexture]"/>
						<param name="unfocusedcolor" value="$PARAM[color]"/>
						<param name="focusedcolor" value="$PARAM[color]"/>
						<param name="border" value="$PARAM[buttonborder]"/>
					</include>
				</control>
				<control type="grouplist">
					<visible>!$PARAM[icons]</visible>
					<right>$PARAM[right]</right>
					<bottom>56</bottom>
					<width>100%</width>
					<height>$PARAM[labelgroupheight]</height>
					<itemgap>6</itemgap>
					<align>right</align>
					<orientation>horizontal</orientation>
					<usecontrolcoords>true</usecontrolcoords>
					<include content="Label_Button_Template">
						<param name="buttonid" value="$PARAM[button1id]"/>
						<param name="color" value="$PARAM[color]"/>
						<param name="focusedfont" value="$PARAM[focusedfont]"/>
						<param name="unfocusedtop" value="$PARAM[unfocusedtop]"/>
						<param name="unfocusedheight" value="$PARAM[unfocusedheight]"/>
						<param name="unfocusedfont" value="$PARAM[unfocusedfont]"/>
					</include>
					<control type="label">
						<visible>!String.IsEmpty(Control.GetLabel($PARAM[button1id])) + Control.IsEnabled($PARAM[button1id]) + Control.IsVisible($PARAM[button1id])</visible>
						<visible>[!String.IsEmpty(Control.GetLabel($PARAM[button2id])) + Control.IsEnabled($PARAM[button2id]) + Control.IsVisible($PARAM[button2id])] | [!String.IsEmpty(Control.GetLabel($PARAM[button3id])) + Control.IsEnabled($PARAM[button3id]) + Control.IsVisible($PARAM[button3id])] | [!String.IsEmpty(Control.GetLabel($PARAM[button4id])) + Control.IsEnabled($PARAM[button4id]) + Control.IsVisible($PARAM[button4id])]</visible>
						<top>$PARAM[unfocusedtop]</top>
						<height>$PARAM[unfocusedheight]</height>
						<width>auto</width>
						<label>[B]/[/B]</label>
						<textcolor>$VAR[Accent_Color]</textcolor>
						<font>$PARAM[unfocusedfont]</font>
						<aligny>center</aligny>
					</control>
					<include content="Label_Button_Template">
						<param name="buttonid" value="$PARAM[button2id]"/>
						<param name="color" value="$PARAM[color]"/>
						<param name="focusedfont" value="$PARAM[focusedfont]"/>
						<param name="unfocusedtop" value="$PARAM[unfocusedtop]"/>
						<param name="unfocusedheight" value="$PARAM[unfocusedheight]"/>
						<param name="unfocusedfont" value="$PARAM[unfocusedfont]"/>
					</include>
					<control type="label">
						<visible>!String.IsEmpty(Control.GetLabel($PARAM[button2id])) + Control.IsEnabled($PARAM[button2id]) + Control.IsVisible($PARAM[button2id])</visible>
						<visible>[!String.IsEmpty(Control.GetLabel($PARAM[button3id])) + Control.IsEnabled($PARAM[button3id]) + Control.IsVisible($PARAM[button3id])] | [!String.IsEmpty(Control.GetLabel($PARAM[button4id])) + Control.IsEnabled($PARAM[button4id]) + Control.IsVisible($PARAM[button4id])]</visible>
						<top>$PARAM[unfocusedtop]</top>
						<width>auto</width>
						<height>$PARAM[unfocusedheight]</height>
						<label>[B]/[/B]</label>
						<textcolor>$VAR[Accent_Color]</textcolor>
						<font>$PARAM[unfocusedfont]</font>
						<aligny>center</aligny>
					</control>
					<include content="Label_Button_Template">
						<param name="buttonid" value="$PARAM[button3id]"/>
						<param name="color" value="$PARAM[color]"/>
						<param name="focusedfont" value="$PARAM[focusedfont]"/>
						<param name="unfocusedtop" value="$PARAM[unfocusedtop]"/>
						<param name="unfocusedheight" value="$PARAM[unfocusedheight]"/>
						<param name="unfocusedfont" value="$PARAM[unfocusedfont]"/>
					</include>
					<control type="label">
						<visible>!String.IsEmpty(Control.GetLabel($PARAM[button3id])) + Control.IsEnabled($PARAM[button3id]) + Control.IsVisible($PARAM[button3id])</visible>
						<visible>!String.IsEmpty(Control.GetLabel($PARAM[button4id])) + Control.IsEnabled($PARAM[button4id]) + Control.IsVisible($PARAM[button4id])</visible>
						<top>$PARAM[unfocusedtop]</top>
						<width>auto</width>
						<height>$PARAM[unfocusedheight]</height>
						<label>[B]/[/B]</label>
						<textcolor>$VAR[Accent_Color]</textcolor>
						<font>$PARAM[unfocusedfont]</font>
						<aligny>center</aligny>
					</control>
					<include content="Label_Button_Template">
						<param name="buttonid" value="$PARAM[button4id]"/>
						<param name="color" value="$PARAM[color]"/>
						<param name="focusedfont" value="$PARAM[focusedfont]"/>
						<param name="unfocusedtop" value="$PARAM[unfocusedtop]"/>
						<param name="unfocusedfont" value="$PARAM[unfocusedfont]"/>
					</include>
				</control>
			</control>
		</definition>
	</include>

</includes>
