<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- textviewer -->
	<defaultcontrol always="true">61</defaultcontrol>
	<include content="WindowOpenClose_Fade_Delayed_Animation"/>
	<controls>
		<include content="Default_Dialog_Panel_Layout">
			<include content="Breadcrumb_Label">
				<param name="title_label_id" value="1"/>
			</include>
			<control type="textbox" id="5">
				<left>120</left>
				<right>120</right>
				<height>645</height>
				<centertop>50%</centertop>
				<aligny>left</aligny>
				<font>Description</font>
				<textcolor>$VAR[Light_Color]</textcolor>
				<pagecontrol>61</pagecontrol>
				<autoscroll>false</autoscroll>
			</control>
			<control type="scrollbar" id="61">
				<visible allowhiddenfocus="true">true</visible>
				<right>26</right>
				<width>6</width>
				<centertop>50%</centertop>
				<height>720</height>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
				<texturesliderbackground colordiffuse="$VAR[Scroll_Background_Color]">views/white_100.png</texturesliderbackground>
				<texturesliderbarfocus colordiffuse="$VAR[List_Scroll_Focus_Color]">views/white_100.png</texturesliderbarfocus>
			</control>
		</include>
	</controls>
</window>
