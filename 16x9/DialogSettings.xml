<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- settingdialog -->
	<defaultcontrol always="true">5</defaultcontrol>
	<backgroundcolor>$VAR[SecondaryColor]</backgroundcolor>
	<include>Delayed_Dialog_Fade_Animation</include>

	<controls>
		<include>DialogFanartSlideshow</include>
		<control type="group">
			<visible>!$EXP[TransparentDialog_IsVisible]</visible>
			<include>Content_Fade_Animation</include>

			<!-- Main group -->
			<control type="group" id="1000">
				<include content="DialogHeading">
					<param name="DialogWidth" value="1820" />
					<param name="DialogHeaderId" value="2" />
				</include>
				<!-- Grouplist -->
				<control type="grouplist" id="5">
					<width>1200</width>
					<include>DialogGrouplist</include>
				</control>
				<!-- Default button -->
				<control type="button" id="7" />
				<!-- Default radiobutton -->
				<control type="radiobutton" id="8" />
				<!-- Default spincontrolex -->
				<control type="spincontrolex" id="9" />
				<!-- Default divider -->
				<control type="image" id="11">
					<top>-1</top>
					<height>1</height>
					<texture></texture>
				</control>
				<!-- Default edit -->
				<control type="edit" id="12" />
				<!-- Default slider -->
				<control type="sliderex" id="13" />
				<!-- Default label -->
				<control type="label" id="14" />
			</control>
			<include content="DialogButtons">
				<param name="Button4Visible" value="false" />
				<!-- Cancel Button -->
				<param name="Button3ID" value="30" />
				<param name="Button3Visible" value="true" />
				<!-- Cancel Button -->
				<param name="Button2ID" value="29" />
				<param name="Button2Visible" value="true" />
				<!-- OK Button -->
				<param name="Button1ID" value="28" />
				<param name="Button1Visible" value="true" />
			</include>
		</control>
	</controls>
</window>
