<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<include name="StripView_ListIcon_Static_Unfocused">
		<definition>
			<include content="ListIcon_Static_Unfocused">
				<param name="visible" value="$PARAM[visible]"/>
				<param name="position" value="$PARAM[position]"/>
				<param name="left" value="$PARAM[left]"/>
				<param name="top" value="$PARAM[top]"/>
				<param name="width" value="$PARAM[width]"/>
				<param name="height" value="$PARAM[height]"/>
				<param name="art_icon" value="$PARAM[art_icon]"/>
				<param name="art_diffuse" value="false"/>
				<param name="now_playing" value="$EXP[ListItem($PARAM[position])_NowPlaying]"/>
				<param name="fallback" value="$PARAM[fallback]"/>
			</include>
		</definition>
	</include>

	<include name="StripView_ListIcon_Static_Focused">
		<param name="border_visible" default="true"/>
		<definition>
			<include content="ListIcon_Static_Focused">
				<param name="visible" value="$PARAM[visible]"/>
				<param name="position" value="$PARAM[position]"/>
				<param name="left" value="$PARAM[left]"/>
				<param name="top" value="$PARAM[top]"/>
				<param name="width" value="$PARAM[width]"/>
				<param name="height" value="$PARAM[height]"/>
				<param name="art_icon" value="$PARAM[art_icon]"/>
				<param name="art_diffuse" value="false"/>
				<param name="now_playing" value="$EXP[ListItem($PARAM[position])_NowPlaying]"/>
				<param name="fallback" value="$PARAM[fallback]"/>
				<!-- Multiart -->
				<include condition="Skin.HasSetting(Multiart_Slideshow) + !Window.Is(movieinformation) + !Window.Is(musicinformation) + !Window.Is(songinformation)" content="Icon_Component_Multiart_Static">
					<param name="texture" value="$VAR[Multiart($PARAM[position])Content]"/>
					<param name="art_diffuse" value="false"/>
					<param name="now_playing" value="$EXP[ListItem($PARAM[position])_NowPlaying]"/>
				</include>
				<!-- Border -->
				<include content="Icon_Component_Border">
					<param name="visible" value="$PARAM[border_visible]"/>
					<param name="size" value="$PARAM[border_size]"/>
					<param name="color" value="$PARAM[border_color]"/>
					<param name="texture" value="$PARAM[border_texture]"/>
				</include>
			</include>
			<!-- Secondary List Mask -->
			<include condition="Skin.HasSetting(Secondary_List_Enabled)" content="Icon_Component_Secondary_Mask">
				<param name="left" value="$PARAM[left]"/>
				<param name="width" value="$PARAM[width]"/>
				<param name="height" value="$PARAM[height]"/>
				<param name="top" value="$PARAM[secondary_mask_top]"/>
				<param name="bottom" value="$PARAM[secondary_mask_bottom]"/>
			</include>
		</definition>
	</include>

	<include name="StripView_ListIcon_Animated">
		<param name="position" default="0"/>
		<definition>
			<include content="ListIcon_Animated">
				<param name="visible" value="$PARAM[visible]"/>
				<param name="position" value="$PARAM[position]"/>
				<param name="left" value="$PARAM[left]"/>
				<param name="top" value="$PARAM[top]"/>
				<param name="width" value="$PARAM[width]"/>
				<param name="height" value="$PARAM[height]"/>
				<param name="art_icon" value="$PARAM[art_icon]"/>
				<param name="art_diffuse" value="false"/>
				<param name="now_playing" value="$EXP[ListItem($PARAM[position])_NowPlaying]"/>
				<param name="fallback" value="$PARAM[fallback]"/>
				<param name="animationtype" value="$PARAM[animationtype]"/>
				<param name="animationeffect" value="$PARAM[animationeffect]"/>
				<param name="animationend" value="$PARAM[animationend]"/>
				<param name="animationcondition" value="$PARAM[animationcondition]"/>
			</include>
		</definition>
	</include>

	<include name="StripView_Multiart_Animated">
		<param name="position" default="0"/>
		<definition>
			<include content="ListIcon_Animated">
				<param name="visible" value="$PARAM[visible]"/>
				<param name="position" value="$PARAM[position]"/>
				<param name="left" value="$PARAM[left]"/>
				<param name="top" value="$PARAM[top]"/>
				<param name="width" value="$PARAM[width]"/>
				<param name="height" value="$PARAM[height]"/>
				<param name="art_icon" value="$PARAM[art_icon]"/>
				<param name="art_diffuse" value="false"/>
				<param name="now_playing" value="$EXP[ListItem($PARAM[position])_NowPlaying]"/>
				<param name="fallback" value="$PARAM[fallback]"/>
				<param name="animationtype" value="$PARAM[animationtype]"/>
				<param name="animationeffect" value="$PARAM[animationeffect]"/>
				<param name="animationend" value="$PARAM[animationend]"/>
				<param name="animationcondition" value="$PARAM[animationcondition]"/>
				<!-- Multiart -->
				<include condition="Skin.HasSetting(Multiart_Slideshow) + !Window.Is(movieinformation) + !Window.Is(musicinformation) + !Window.Is(songinformation)" content="Icon_Component_Multiart_Animated">
					<param name="visible" value="$PARAM[multiart]"/>
					<param name="texture" value="$VAR[Multiart($PARAM[position])Content]"/>
					<param name="art_diffuse" value="false"/>
					<param name="now_playing" value="$EXP[ListItem($PARAM[position])_NowPlaying]"/>
					<param name="animationtype" value="$PARAM[multiartanimationtype]"/>
					<param name="animationeffect" value="$PARAM[multiartanimationeffect]"/>
					<param name="animationend" value="$PARAM[multiartanimationend]"/>
					<param name="animationcondition" value="$PARAM[multiartanimationcondition]"/>
				</include>
			</include>
		</definition>
	</include>

	<include name="StripView_Animated">
		<param name="target_id"/>
		<param name="displaytype_label"/>
		<param name="top"/>
		<param name="item_top"/>
		<param name="item_width"/>
		<param name="item_height"/>
		<param name="focus_width"/>
		<param name="focus_height"/>
		<param name="2x_item_width"/>
		<param name="2x_item_width_plus_focus_width"/>
		<param name="2x_item_width_minus_dropshadow_width"/>
		<param name="3x_item_width_plus_focus_width"/>
		<param name="4x_item_width_plus_focus_width"/>
		<param name="art_icon"/>
		<param name="fallback"/>
		<definition>
			<control type="group">
				<visible>$EXP[StripView_Display_Is$PARAM[displaytype_label]] + !$EXP[ContainerSwitching]</visible>
				<include>Artwork_Fade_Animation</include>
				<animation effect="slide" start="0,0" end="-960,0" time="0" condition="$EXP[StripView_Display_IsFanart] + !$EXP[InfoWidget_Display_IsStripView_Posters] + !$EXP[InfoWidget_Display_IsStripView_Square]">Conditional</animation>
				<top>$PARAM[top]</top>
				<!-- On Previous -->
				<control type="group">
					<visible>$EXP[ContentPrevious] + !String.IsEqual(Container($PARAM[target_id]).NumItems,1)</visible>
					<!-- Move Icon -3 to -2 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious + [[$EXP[StripView_Details_Off] + !$EXP[StripView_Display_IsFanart]] | [$EXP[InfoDialog_IsVisible] + !$EXP[InfoWidget_Display_IsStripView_Posters]]]"/>
						<param name="position" value="-2"/>
						<param name="left" value="-$PARAM[item_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="$PARAM[item_width],0"/>
					</include>
					<!-- Move Icon -2 to -1 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious + [$EXP[StripView_Details_Off] | $EXP[InfoWidget_Display_IsStripView]]"/>
						<param name="position" value="-1"/>
						<param name="left" value="0"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="$PARAM[item_width],0"/>
					</include>
					<!-- Move Icon -1 to 0 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious + [$EXP[StripView_Details_Off] | $EXP[InfoWidget_Display_IsStripView]]"/>
						<param name="position" value="0"/>
						<param name="left" value="$PARAM[item_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="zoom"/>
						<param name="animationend" value="$PARAM[2x_item_width],0,$PARAM[focus_width],$PARAM[focus_height]"/>
					</include>
					<!-- Move Icon -1 to 0 (Alternative Animation if Details On) -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious  + !$EXP[StripView_Details_Off] + !$EXP[InfoWidget_Display_IsStripView] + !Window.IsVisible(loginscreen)"/>
						<param name="position" value="0"/>
						<param name="left" value="-$PARAM[focus_width]"/>
						<param name="top" value="0"/>
						<param name="width" value="$PARAM[focus_width]"/>
						<param name="height" value="$PARAM[focus_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="$PARAM[2x_item_width_plus_focus_width],0"/>
					</include>
					<!-- Move Icon 0 to 1 -->
					<include content="StripView_Multiart_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious"/>
						<param name="position" value="1"/>
						<param name="left" value="$PARAM[2x_item_width]"/>
						<param name="top" value="0"/>
						<param name="width" value="$PARAM[focus_width]"/>
						<param name="height" value="$PARAM[focus_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="zoom"/>
						<param name="animationend" value="$PARAM[2x_item_width_plus_focus_width],$PARAM[item_top],$PARAM[item_width],$PARAM[item_height]"/>
						<param name="multiart" value="Container($PARAM[target_id]).OnPrevious + String.IsEqual(Window(home).Property(Multiart_Fadeout),true)"/>
						<param name="multiartanimationtype" value="Hidden"/>
						<param name="multiartanimationeffect" value="fade"/>
						<param name="multiartanimationend" value="0"/>
						<param name="multiartanimationcondition" value="true"/>
					</include>
					<!-- Move Icon 1 to 2 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious"/>
						<param name="position" value="2"/>
						<param name="left" value="$PARAM[2x_item_width_plus_focus_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="$PARAM[item_width],0"/>
					</include>
					<!-- Move Icon 2 to 3 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious + !$EXP[StripView_Display_IsFanart] + !$EXP[InfoWidget_Display_IsStripView_Fanart]"/>
						<param name="position" value="3"/>
						<param name="left" value="$PARAM[3x_item_width_plus_focus_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="$PARAM[item_width],0"/>
					</include>
					<!-- Dropshadow for icon 0 to 1 -->
					<!-- Left Side -->
					<include content="DropShadow">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious + ![String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(1).Label) + String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(1).Art(thumb))] + [$EXP[StripView_Details_Off] | $EXP[InfoWidget_Display_IsStripView]]"/>
						<param name="left" value="$PARAM[2x_item_width_minus_dropshadow_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="zoom"/>
						<param name="animationend" value="$PARAM[2x_item_width_plus_focus_width],0,0,$PARAM[focus_height]"/>
						<param name="animation2effect" value="zoom"/>
						<param name="animation2end" value="0,100"/>
						<param name="animation2time" value="210"/>
						<param name="animation2center" value="$PARAM[2x_item_width],540"/>
						<param name="animation3effect" value="fade"/>
						<param name="animation3end" value="0"/>
						<param name="animation3time" value="210"/>
					</include>
					<!-- Right Side -->
					<include content="DropShadow">
						<param name="visible" value="Container($PARAM[target_id]).OnPrevious + ![String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(2).Label) + String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(2).Art(thumb))]"/>
						<param name="left" value="$PARAM[2x_item_width_plus_focus_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="flipx" value="true"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="zoom"/>
						<param name="animationend" value="$PARAM[3x_item_width_plus_focus_width],$PARAM[item_top],0,$PARAM[item_height]"/>
						<param name="animation2effect" value="fade"/>
						<param name="animation2end" value="0"/>
						<param name="animation2delay" value="20"/>
						<param name="animation2time" value="360"/>
					</include>
					<!-- Dropshadow for icon -1 to 0 -->
					<!-- Left Side -->
					<include content="DropShadow">
						<param name="visible" value="!Container($PARAM[target_id]).OnPrevious + ![String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(-1).Label) + String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(-1).Art(thumb))] + [$EXP[StripView_Details_Off] | $EXP[InfoWidget_Display_IsStripView]]"/>
						<param name="left" value="$PARAM[item_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="1"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="animationtype" value="Visible"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="zoom"/>
						<param name="animationend" value="$PARAM[2x_item_width_minus_dropshadow_width],$PARAM[item_top],120,$PARAM[item_height]"/>
						<param name="animation2effect" value="fade"/>
						<param name="animation2start" value="0"/>
						<param name="animation2end" value="100"/>
						<param name="animation2delay" value="20"/>
						<param name="animation2time" value="360"/>
					</include>
					<!-- Dropshadow for icon -1 to 0 -->
					<!-- Right Side -->
					<include content="DropShadow">
						<param name="visible" value="!Container($PARAM[target_id]).OnPrevious + ![String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(1).Label) + String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(1).Art(thumb))]"/>
						<param name="left" value="$PARAM[2x_item_width]"/>
						<param name="top" value="0"/>
						<param name="width" value="1"/>
						<param name="height" value="$PARAM[focus_height]"/>
						<param name="flipx" value="true"/>
						<param name="animationtype" value="Visible"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="zoom"/>
						<param name="animationend" value="$PARAM[2x_item_width_plus_focus_width],$PARAM[item_top],120,$PARAM[item_height]"/>
						<param name="animation2effect" value="fade"/>
						<param name="animation2start" value="0"/>
						<param name="animation2end" value="100"/>
						<param name="animation2delay" value="210"/>
						<param name="animation2time" value="170"/>
					</include>
				</control>
				<!-- On Next -->
				<control type="group">
					<visible>$EXP[ContentNext] + !String.IsEqual(Container($PARAM[target_id]).NumItems,1)</visible>
					<!-- Move Icon -2 to -3 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnNext + [[$EXP[StripView_Details_Off] + !$EXP[StripView_Display_IsFanart]] | [$EXP[InfoDialog_IsVisible] + !$EXP[InfoWidget_Display_IsStripView_Posters]]]"/>
						<param name="position" value="-3"/>
						<param name="left" value="0"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="-$PARAM[item_width],0"/>
					</include>
					<!-- Move Icon -1 to -2 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnNext + [$EXP[StripView_Details_Off] | $EXP[InfoWidget_Display_IsStripView]]"/>
						<param name="position" value="-2"/>
						<param name="left" value="$PARAM[item_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="-$PARAM[item_width],0"/>
					</include>
					<!-- Move Icon 0 to -1 -->
					<include content="StripView_Multiart_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnNext + [$EXP[StripView_Details_Off] | $EXP[InfoWidget_Display_IsStripView]]"/>
						<param name="position" value="-1"/>
						<param name="left" value="$PARAM[2x_item_width]"/>
						<param name="top" value="0"/>
						<param name="width" value="$PARAM[focus_width]"/>
						<param name="height" value="$PARAM[focus_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="zoom"/>
						<param name="animationend" value="$PARAM[item_width],$PARAM[item_top],$PARAM[item_width],$PARAM[item_height]"/>
						<param name="multiart" value="Container($PARAM[target_id]).OnNext + String.IsEqual(Window(home).Property(Multiart_Fadeout),true)"/>
						<param name="multiartanimationtype" value="Hidden"/>
						<param name="multiartanimationeffect" value="fade"/>
						<param name="multiartanimationend" value="0"/>
						<param name="multiartanimationcondition" value="true"/>
					</include>
					<!-- Alternative Animation if Details On -->
					<include content="StripView_Multiart_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnNext + !$EXP[StripView_Details_Off] + !$EXP[InfoWidget_Display_IsStripView] + !Window.IsVisible(loginscreen)"/>
						<param name="position" value="-1"/>
						<param name="left" value="$PARAM[2x_item_width]"/>
						<param name="top" value="0"/>
						<param name="width" value="$PARAM[focus_width]"/>
						<param name="height" value="$PARAM[focus_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="-$PARAM[2x_item_width_plus_focus_width],0"/>
						<param name="multiart" value="Container($PARAM[target_id]).OnNext + String.IsEqual(Window(home).Property(Multiart_Fadeout),true)"/>
						<param name="multiartanimationtype" value="Hidden"/>
						<param name="multiartanimationeffect" value="fade"/>
						<param name="multiartanimationend" value="0"/>
						<param name="multiartanimationcondition" value="true"/>
					</include>
					<!-- Move Icon 1 to 0 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnNext"/>
						<param name="position" value="0"/>
						<param name="left" value="$PARAM[2x_item_width_plus_focus_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="zoom"/>
						<param name="animationend" value="$PARAM[2x_item_width],0,$PARAM[focus_width],$PARAM[focus_height]"/>
					</include>
					<!-- Move Icon 2 to 1 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnNext"/>
						<param name="position" value="1"/>
						<param name="left" value="$PARAM[3x_item_width_plus_focus_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="animationtype" value="Hidden"/>
						<param name="animationcondition" value="true"/>
						<param name="animationeffect" value="slide"/>
						<param name="animationend" value="-$PARAM[item_width],0"/>
					</include>
					<!-- Move Icon 3 to 2 -->
					<include content="StripView_ListIcon_Animated">
						<param name="visible" value="Container($PARAM[target_id]).OnNext + [!$EXP[StripView_Display_IsFanart] | $EXP[InfoWidget_Display_IsStripView_Posters] | $EXP[InfoWidget_Display_IsStripView_Square]]"/>
						<param name="position" value="2"/>
						<param name="left" value="$PARAM[4x_item_width_plus_focus_width]"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
