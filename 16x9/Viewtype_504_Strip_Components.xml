				<param name="focus_height" value="$PARAM[focus_height]"/>
				<param name="2x_item_width" value="$PARAM[2x_item_width]"/>
				<param name="2x_item_width_plus_focus_width" value="$PARAM[2x_item_width_plus_focus_width]"/>
