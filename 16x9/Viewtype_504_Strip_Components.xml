	<include name="StripView_Static">
		<param name="target_id"/>
		<param name="displaytype_label"/>
		<param name="left" default="0"/>
		<param name="top" default="0"/>
		<param name="item_top"/>
		<param name="item_width"/>
		<param name="item_height"/>
		<param name="focus_width"/>
		<param name="focus_height"/>
		<param name="2x_item_width"/>
		<param name="2x_item_width_plus_focus_width"/>
		<param name="3x_item_width_plus_focus_width"/>
		<param name="2x_item_width_minus_dropshadow_width"/>
		<param name="art_icon"/>
		<param name="fallback"/>
		<param name="progress_overlay" default="false"/>
		<param name="focus_border_visible"/>
		<param name="focus_border_protrusion"/>
		<param name="focus_border_size"/>
		<param name="focus_border_color"/>
		<param name="focus_border_texture"/>
		<param name="secondary_mask_top"/>
		<param name="secondary_mask_bottom"/>
		<definition>
			<control type="group">
				<visible>$EXP[StripView_Display_Is$PARAM[displaytype_label]] + !$EXP[ContainerSwitching] + !$EXP[OptionsSwitching]</visible>
				<include>Artwork_Fade_Animation</include>
				<left>$PARAM[left]</left>
				<top>$PARAM[top]</top>
				<control type="group">
					<visible>!$EXP[ContentSwitching]</visible>
					<!-- Unfocused -->
					<include content="StripView_ListIcon_Static_Unfocused">
						<param name="visible" value="[!$EXP[StripView_Display_IsFanart] + $EXP[StripView_Details_Off]] | $EXP[InfoWidget_Display_IsStripView_Posters] | $EXP[InfoWidget_Display_IsStripView_Square] | Window.IsVisible(loginscreen)"/>
						<param name="position" value="-2"/>
						<param name="left" value="0"/>
						<param name="top" value="$PARAM[item_top]"/>
						<param name="width" value="$PARAM[item_width]"/>
						<param name="height" value="$PARAM[item_height]"/>
						<param name="art_icon" value="$PARAM[art_icon]"/>
						<param name="fallback" value="$PARAM[fallback]"/>
						<param name="progress_overlay" value="$PARAM[progress_overlay]"/>
					</include>
					<control type="group">
						<animation effect="slide" start="0,0" end="-$PARAM[item_width],0" time="0" condition="$EXP[StripView_Display_IsFanart] + !$EXP[InfoWidget_Display_IsStripView_Posters] + !$EXP[InfoWidget_Display_IsStripView_Square]">Conditional</animation>
						<include content="StripView_ListIcon_Static_Unfocused">
							<param name="visible" value="$EXP[StripView_Details_Off] | $EXP[InfoWidget_Display_IsStripView] | Window.IsVisible(loginscreen)"/>
							<param name="position" value="-1"/>
							<param name="left" value="$PARAM[item_width]"/>
							<param name="top" value="$PARAM[item_top]"/>
							<param name="width" value="$PARAM[item_width]"/>
							<param name="height" value="$PARAM[item_height]"/>
							<param name="art_icon" value="$PARAM[art_icon]"/>
							<param name="fallback" value="$PARAM[fallback]"/>
							<param name="progress_overlay" value="$PARAM[progress_overlay]"/>
						</include>
					</control>
					<control type="group">
						<animation effect="slide" start="0,0" end="-$PARAM[item_width],0" time="0" condition="$EXP[StripView_Display_IsFanart] + !$EXP[InfoWidget_Display_IsStripView_Posters] + !$EXP[InfoWidget_Display_IsStripView_Square]">Conditional</animation>
						<include content="StripView_ListIcon_Static_Unfocused">
							<param name="visible" value="true"/>
							<param name="position" value="1"/>
							<param name="left" value="$PARAM[2x_item_width_plus_focus_width]"/>
							<param name="top" value="$PARAM[item_top]"/>
							<param name="width" value="$PARAM[item_width]"/>
							<param name="height" value="$PARAM[item_height]"/>
							<param name="art_icon" value="$PARAM[art_icon]"/>
							<param name="fallback" value="$PARAM[fallback]"/>
							<param name="progress_overlay" value="$PARAM[progress_overlay]"/>
						</include>
						<include content="StripView_ListIcon_Static_Unfocused">
							<param name="visible" value="!$EXP[StripView_Display_IsFanart] | $EXP[InfoWidget_Display_IsStripView_Posters] | $EXP[InfoWidget_Display_IsStripView_Square] | Window.IsVisible(loginscreen)"/>
							<param name="position" value="2"/>
							<param name="left" value="$PARAM[3x_item_width_plus_focus_width]"/>
							<param name="top" value="$PARAM[item_top]"/>
							<param name="width" value="$PARAM[item_width]"/>
							<param name="height" value="$PARAM[item_height]"/>
							<param name="art_icon" value="$PARAM[art_icon]"/>
							<param name="fallback" value="$PARAM[fallback]"/>
							<param name="progress_overlay" value="$PARAM[progress_overlay]"/>
						</include>
						<!-- Dropshadow -->
						<include content="DropShadow">
							<param name="visible" value="!$EXP[ShowcaseView_IsVisible] + ![$EXP[StripView_IsVisible] + !$EXP[InfoWidget_Display_IsStripView] + !$EXP[StripView_Details_Off] + !Window.IsVisible(loginscreen)] + ![String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(-1).Label) + String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(-1).Art(thumb))]"/>
							<param name="left" value="$PARAM[2x_item_width_minus_dropshadow_width]"/>
							<param name="top" value="$PARAM[item_top]"/>
							<param name="height" value="$PARAM[item_height]"/>
						</include>
						<include content="DropShadow">
							<param name="visible" value="![$EXP[ShowcaseView_IsVisible] + [!$EXP[ShowcaseView_Details_Off] | Control.HasFocus(60) | Control.HasFocus(3000) | $EXP[InfoWidget_Display_IsShowcaseView]]] + ![String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(1).Label) + String.IsEmpty(Container($PARAM[target_id]).ListItemNoWrap(1).Art(thumb))]"/>
							<param name="left" value="$PARAM[2x_item_width_plus_focus_width]"/>
							<param name="top" value="$PARAM[item_top]"/>
							<param name="height" value="$PARAM[item_height]"/>
							<param name="flipx" value="true"/>
						</include>
						<!-- Focused -->
						<include content="StripView_ListIcon_Static_Focused">
							<param name="position" value="0"/>
							<param name="left" value="$PARAM[2x_item_width]"/>
							<param name="width" value="$PARAM[focus_width]"/>
							<param name="height" value="$PARAM[focus_height]"/>
							<param name="art_icon" value="$PARAM[art_icon]"/>
							<param name="fallback" value="$PARAM[fallback]"/>
							<param name="progress_overlay" value="$PARAM[progress_overlay]"/>
							<param name="border_visible" value="$PARAM[focus_border_visible]"/>
							<param name="border_protrusion" value="$PARAM[focus_border_protrusion]"/>
							<param name="border_size" value="$PARAM[focus_border_size]"/>
							<param name="border_color" value="$PARAM[focus_border_color]"/>
							<param name="border_texture" value="$PARAM[focus_border_texture]"/>
							<param name="secondary_mask_top" value="$PARAM[secondary_mask_top]"/>
							<param name="secondary_mask_bottom" value="$PARAM[secondary_mask_bottom]"/>
						</include>
					</control>
				</control>
			</control>
		</definition>
	</include>

