					<include content="Transparent_Dialog_Panel">
						<param name="top" value="180"/>
						<param name="bottom" value="645"/>
					</include>
				</control>

				<control type="panel" id="9005">
					<animation effect="fade" start="0" end="100" time="360" delay="360" tween="sine" easing="inout" condition="Integer.IsGreater(Container(9005).NumItems,0) | Container(9005).IsUpdating">Conditional</animation>
