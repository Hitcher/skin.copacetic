<?xml version="1.0" encoding="UTF-8"?>
<window>
	<include>TransparentDialogWindowFadeAnimation</include>
	<controls>

		<!--Hidden Labels-->
		<control type="label" id="30">
			<visible allowhiddenfocus="true">false</visible>
		</control>
		<control type="label" id="31">
			<visible allowhiddenfocus="true">false</visible>
		</control>
		<control type="progress" id="32">
			<visible allowhiddenfocus="true">false</visible>
		</control>

		<include content="IconDetails">
			<param name="progressbarvisible" value="true"/>
			<param name="progressbarinfo" value="Control.GetLabel(32)"/>
			<param name="titlelabel" value="$INFO[Control.GetLabel(30)]"/>
			<param name="infolabel" value="$INFO[Control.GetLabel(31)]"/>
		</include>

	</controls>
</window>
