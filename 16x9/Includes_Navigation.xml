	<include name="List_Navigation">
		<definition>
			<onup condition="Integer.IsGreater(Container.NumItems,1)">50</onup>
			<onup condition="Integer.IsEqual(Container.NumItems,1)">SetFocus(3000,0)</onup>
			<ondown condition="Integer.IsGreater(Container.NumItems,1)">50</ondown>
			<ondown condition="Integer.IsEqual(Container.NumItems,1)">SetFocus(3000,0)</ondown>
			<onleft condition="$EXP[Secondary_List_Focus] + Integer.IsGreater(Container(4501).NumItems,0)">SetFocus(4501,0)</onleft>
			<onleft condition="![$EXP[Secondary_List_Focus] + Integer.IsGreater(Container(4501).NumItems,0)]">SetFocus(3000,0)</onleft>
			<onright condition="Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio">SetFocus(91100,1)</onright>
			<onright condition="![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio]  + Integer.IsGreater(Container.NumItems,12)">61</onright>
			<onright condition="![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio]  + Integer.IsLess(Container.NumItems,12)">SetFocus(3000,0)</onright>
			<onback condition="String.IsEqual(window(movieinformation).property(SentFromInfo),true)">SetFocus(5005)</onback>
		</definition>
	</include>

	<include name="Simple_List_Navigation">
		<definition>
			<onleft condition="Window.IsActive(favourites)">0</onleft>
			<onleft condition="Window.IsActive(smartplaylisteditor)">100</onleft>
			<onleft condition="Window.IsActive(filemanager) + Control.HasFocus(20)">SetFocus(9000)</onleft>
			<onleft condition="Window.IsActive(filemanager) + Control.HasFocus(21)">SetFocus(20)</onleft>
			<onright condition="Window.IsActive(favourites) + Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio">SetFocus(91100,1)</onright>
			<onright condition="Window.IsActive(favourites) + ![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio]">0</onright>
			<onright condition="Window.IsActive(smartplaylisteditor) + !Window.IsActive(smartplaylistrule) + !Window.IsActive(selectdialog) + !String.IsEqual(Container(10).ListItem.Label,New rule...)">14</onright>
			<onright condition="Window.IsActive(smartplaylisteditor) + String.IsEqual(Container(10).ListItem.Label,New rule...)">9000</onright>
			<onright condition="Window.IsActive(filemanager) + Control.HasFocus(20)">SetFocus(21)</onright>
			<onright condition="Window.IsActive(filemanager) + !Control.HasFocus(20)">SetFocus(9000)</onright>
			<onright condition="Window.IsActive(selectdialog) | Window.IsActive(filebrowser)">SetFocus(9000)</onright>
			<onright condition="!Window.IsVisible(favourites) + !Window.IsVisible(smartplaylisteditor) + !Window.IsVisible(selectdialog) + !Window.IsVisible(filebrowser) + !Window.IsVisible(filemanager)">9000</onright>
			<onleft condition="Window.IsActive(musicinformation)">SetFocus(65000)</onleft>
			<onright condition="Window.IsActive(musicinformation)">SetFocus(65000)</onright>
