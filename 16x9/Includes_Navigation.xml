	<include name="Grid_Navigation">
		<definition>
			<onleft>SetFocus(3000,0)</onleft>
			<onright condition="Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio">SetFocus(91100,1)</onright>
			<onright condition="![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio] + $EXP[GridView_Display_IsFanart] + Integer.IsGreater(Container.NumItems,3)">61</onright>
			<onright condition="![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio] + $EXP[GridView_Display_IsPosters] + Integer.IsGreater(Container.NumItems,5)">61</onright>
			<onright condition="![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio] + $EXP[GridView_Display_IsSquare] + Integer.IsGreater(Container.NumItems,10)">61</onright>
			<onright condition="![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio]  + $EXP[GridView_Display_IsFanart] + Integer.IsLessorEqual(Container.NumItems,3)">SetFocus(3000,0)</onright>
			<onright condition="![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio]  + $EXP[GridView_Display_IsPosters] + Integer.IsLessorEqual(Container.NumItems,5)">SetFocus(3000,0)</onright>
			<onright condition="![Skin.HasSetting(Music_Player_Enabled) + Player.HasAudio]  + $EXP[GridView_Display_IsSquare] + Integer.IsLessorEqual(Container.NumItems,10)">SetFocus(3000,0)</onright>
			<onup condition="$EXP[GridView_Display_IsFanart] + Integer.IsGreater(Container.NumItems,3)">505</onup>
			<onup condition="!$EXP[GridView_Display_IsFanart] + Integer.IsGreater(Container.NumItems,5)">505</onup>
			<onup condition="$EXP[GridView_Display_IsFanart] + Integer.IsLessorEqual(Container.NumItems,3)">0</onup>
			<onup condition="!$EXP[GridView_Display_IsFanart] + Integer.IsLessorEqual(Container.NumItems,5)">0</onup>
			<ondown condition="$EXP[GridView_Display_IsFanart] + Integer.IsGreater(Container.NumItems,3)">505</ondown>
			<ondown condition="!$EXP[GridView_Display_IsFanart] + Integer.IsGreater(Container.NumItems,5)">505</ondown>
			<ondown condition="$EXP[GridView_Display_IsFanart] + Integer.IsLessorEqual(Container.NumItems,3)">0</ondown>
			<ondown condition="!$EXP[GridView_Display_IsFanart] + Integer.IsLessorEqual(Container.NumItems,5)">0</ondown>
			<onback condition="String.IsEqual(window(movieinformation).property(SentFromInfo),true)">SetFocus(5005)</onback>
		</definition>
	</include>

</includes>
