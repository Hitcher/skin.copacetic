	<include name="SettingsCategory_Layout">
		<param name="categoryid" default="30000"/>
		<param name="mainlist"/>
		<param name="sublist"/>
		<param name="iconborder" default="false"/>
		<definition>

			<control type="group">
				<include content="Breadcrumb_Label">
					<param name="left" value="90"/>
					<param name="visible" value="!Window.IsVisible(textviewer)"/>
					<param name="label_color" value="$VAR[Light_Color]"/>
					<param name="info_label_visible" value="false"/>
				</include>
			</control>

			<!-- Addon screen -->
			<control type="label">
				<visible>Control.HasFocus(3000010)</visible>
				<include>Delayed_Content_Fade_Animation</include>
				<align>center</align>
				<left>240</left>
				<right>120</right>
				<height>60</height>
				<centertop>50%</centertop>
				<textcolor>$VAR[Light_Color]</textcolor>
				<font>Breadcrumb_Title_Label</font>
				<label>$LOCALIZE[31104]</label>
			</control>

			<!-- Arrows when focused -->
			<control type="group">
				<animation effect="zoom" center="auto" start="112" end="100" time="135" tween="back" easing="in" condition="ControlGroup($PARAM[categoryid]).HasFocus">Conditional</animation>
				<visible>ControlGroup($PARAM[categoryid]).HasFocus</visible>
				<left>30</left>
				<width>60</width>
				<top>30</top>
				<height>120</height>
				<control type="image">
					<top>0</top>
					<width>30</width>
					<height>30</height>
					<texture colordiffuse="$VAR[Light_Color]">settings/category_switcher.png</texture>
				</control>
				<control type="image">
					<animation effect="rotate" center="auto" end="180" time="0" condition="true" reversible="false">Conditional</animation>
					<bottom>0</bottom>
					<width>30</width>
					<height>30</height>
					<texture colordiffuse="$VAR[Light_Color]">settings/category_switcher.png</texture>
					<aspectratio>keep</aspectratio>
				</control>
			</control>

			<!-- Icon -->
			<control type="image">
				<top>60</top>
				<left>15</left>
				<width>60</width>
				<height>60</height>
				<texture colordiffuse="$VAR[Light_Color]">$VAR[SettingsCategory_Icon]</texture>
			</control>

			<control type="grouplist" id="$PARAM[categoryid]">
				<visible allowhiddenfocus="true">false</visible>
				<top>60</top>
				<left>15</left>
				<width>60</width>
				<height>60</height>
				<orientation>vertical</orientation>
				<onleft condition="!$PARAM[buttonvisible]">$PARAM[sublist]</onleft>
				<onleft condition="$PARAM[buttonvisible]">20</onleft>
				<onright>$PARAM[mainlist]</onright>
				<align>center</align>
				<itemgap>0</itemgap>
				<scrolltime>0</scrolltime>
				<control type="button" id="$PARAM[categoryid]0">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(skinsettings)">ReplaceWindowAndFocus(skinsettings,$PARAM[categoryid]0)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]1">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(interfacesettings)">ReplaceWindowAndFocus(interfacesettings,$PARAM[categoryid]1)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]2">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(mediasettings)">ReplaceWindowAndFocus(mediasettings,$PARAM[categoryid]2)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]3">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(playersettings)">ReplaceWindowAndFocus(playersettings,$PARAM[categoryid]3)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]4">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(systemsettings)">ReplaceWindowAndFocus(systemsettings,$PARAM[categoryid]4)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]5">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(servicesettings)">ReplaceWindowAndFocus(servicesettings,$PARAM[categoryid]5)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]6">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(gamesettings)">ReplaceWindowAndFocus(gamesettings,$PARAM[categoryid]6)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]7">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(pvrsettings)">ReplaceWindowAndFocus(pvrsettings,$PARAM[categoryid]7)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]8">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(profiles)">ReplaceWindowAndFocus(profiles,$PARAM[categoryid]8)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]9">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(systeminfo)">ReplaceWindowAndFocus(systeminfo,$PARAM[categoryid]9)</onfocus>
				</control>
				<control type="button" id="$PARAM[categoryid]10">
					<onclick>ClearProperty(SettingsCategory_Switcher,home)</onclick>
					<onclick>ReplaceWindow(addonbrowser)</onclick>
					<onright>noop</onright>
					<onleft>noop</onleft>
				</control>
				<control type="button" id="$PARAM[categoryid]11">
					<onfocus>SetProperty(SettingsCategory_Switcher,true,home)</onfocus>
					<onfocus condition="!Window.Is(filemanager)">ReplaceWindowAndFocus(filemanager,$PARAM[categoryid]11)</onfocus>
				</control>
			</control>
		</definition>
