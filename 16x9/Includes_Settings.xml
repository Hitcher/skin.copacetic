<includes>

	<!-- Generic -->
	<include name="Settings_Left_List_Template">
		<param name="windowname"/>
		<param name="id"/>
		<param name="scrollbarid"/>
		<param name="left" default="60"/>
		<param name="dividerleft" default="780"/>
		<param name="width" default="720"/>
		<param name="height" default="630"/>
		<param name="onleft" default="0"/>
		<param name="onright" default="0"/>
		<param name="focusbordervisible" default="true"/>
		<param name="label" default="$INFO[ListItem.Label]"/>
		<param name="label2" default="$INFO[ListItem.Label2]"/>
		<definition>
			<control type="list" id="$PARAM[id]">
				<left>$PARAM[left]</left>
				<width>$PARAM[width]</width>
				<height>$PARAM[height]</height>
				<centertop>50%</centertop>
				<onfocus>SetProperty(ActiveList,Left,$PARAM[windowname])</onfocus>
				<onleft>$PARAM[onleft]</onleft>
				<onright condition="[Container($PARAM[id]).HasPrevious | Container($PARAM[id]).HasNext]">$PARAM[scrollbarid]</onright>
				<onright condition="![Container($PARAM[id]).HasPrevious | Container($PARAM[id]).HasNext]">$PARAM[onright]</onright>
				<onup>$PARAM[id]</onup>
				<ondown>$PARAM[id]</ondown>
				<orientation>vertical</orientation>
				<align>top</align>
				<pagecontrol>$PARAM[scrollbarid]</pagecontrol>
				<scrolltime tween="sine" easing="inout">360</scrolltime>
				<itemlayout height="90" width="$PARAM[width]">
					<control type="label">
						<textoffsetx>60</textoffsetx>
						<label>$PARAM[label]</label>
						<textcolor>$VAR[Unfocused_Color]</textcolor>
					</control>
					<control type="label">
						<textoffsetx>60</textoffsetx>
						<align>right</align>
						<label>$PARAM[label2]</label>
						<textcolor>$VAR[Unfocused_Color]</textcolor>
					</control>
				</itemlayout>
				<focusedlayout height="90" width="$PARAM[width]">
					<control type="label">
						<textoffsetx>60</textoffsetx>
						<label>$PARAM[label]</label>
						<textcolor>$VAR[Left_Grouplist_Focus_Color]</textcolor>
					</control>
					<control type="label">
						<textoffsetx>60</textoffsetx>
						<align>right</align>
						<label>$PARAM[label2]</label>
						<textcolor>$VAR[Left_Grouplist_Focus_Color]</textcolor>
					</control>
					<control type="image">
						<visible>$PARAM[focusbordervisible]</visible>
						<left>60</left>
						<right>0</right>
						<height>1</height>
						<bottom>0</bottom>
						<texture colordiffuse="$VAR[Unfocused_Color]">views/white_100.png</texture>
					</control>
				</focusedlayout>
			</control>
			<include content="Dialog_Divider_With_Scrollbar">
				<param name="scrollbarvisible" value="Control.HasFocus($PARAM[id]) | Control.HasFocus($PARAM[scrollbarid])"/>
				<param name="id" value="$PARAM[scrollbarid]"/>
				<param name="height" value="$PARAM[height]"/>
				<param name="left" value="$PARAM[dividerleft]"/>
				<param name="scrollbarleft" value="1"/>
				<param name="onleft" value="$PARAM[id]"/>
				<param name="onright" value="$PARAM[onright]"/>
			</include>
		</definition>
	</include>

