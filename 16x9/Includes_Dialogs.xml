						<param name="label" value="$PARAM[button15label]"/>
					</include>
					<include content="Keyboard_Label">
						<param name="id" value="$PARAM[button16id]"/>
						<param name="label" value="$PARAM[button16label]"/>
					</include>
					<include content="Keyboard_Label">
						<param name="id" value="$PARAM[button17id]"/>
						<param name="label" value="$PARAM[button17label]"/>
					</include>
