	<include name="Keyboard_Label_Button">
		<param name="id" default=""/>
		<param name="position" default=""/>
		<param name="width" default="120"/>
		<definition>
			<control type="group">
				<height>$PARAM[width]</height>
				<width>$PARAM[width]</width>
				<control type="group">
					<visible>Control.HasFocus($PARAM[id])</visible>
					<control type="image">
						<animation effect="slide" end="0,5" time="0" reversible="false" condition="true">Conditional</animation>
						<visible>Container($PARAM[id]).Position($PARAM[position])</visible>
						<left>15</left>
						<right>15</right>
						<bottom>0</bottom>
						<height>10</height>
						<texture colordiffuse="$VAR[Accent_Color]">views/white_100.png</texture>
					</control>
					<control type="label">
						<align>center</align>
						<aligny>center</aligny>
						<label>$INFO[Container($PARAM[id]).ListItemNoWrap($PARAM[position]).Label]</label>
						<font>Keyboard_Button_$PARAM[position]</font>
						<textcolor>$VAR[Light_Color]</textcolor>
					</control>
				</control>
				<control type="label">
					<visible>!Control.HasFocus($PARAM[id])</visible>
					<align>center</align>
					<aligny>center</aligny>
					<label>$INFO[Container($PARAM[id]).ListItemNoWrap($PARAM[position]).Label]</label>
					<font>Keyboard_Button_5</font>
					<textcolor>$VAR[Unfocused_Color]</textcolor>
				</control>
			</control>
		</definition>
	</include>

	<include name="Numeric_Label_Button">
		<param name="id" default="1000"/>
		<param name="position" default=""/>
		<param name="width" default="120"/>
		<definition>
			<control type="group">
				<width>120</width>
				<height>120</height>
				<control type="group">
					<visible>Control.HasFocus($PARAM[id]) + Container($PARAM[id]).Position($PARAM[position])</visible>
					<control type="label">
						<align>center</align>
						<aligny>center</aligny>
						<label>$INFO[Container($PARAM[id]).ListItemPosition($PARAM[position]).Label]</label>
						<font>Keyboard_Button_0</font>
						<textcolor>$VAR[Light_Color]</textcolor>
					</control>
					<control type="image">
						<left>15</left>
						<right>15</right>
						<bottom>0</bottom>
						<height>10</height>
						<texture colordiffuse="$VAR[Accent_Color]">views/white_100.png</texture>
					</control>
				</control>
				<control type="label">
					<visible>![Control.HasFocus($PARAM[id]) + Container($PARAM[id]).Position($PARAM[position])]</visible>
					<align>center</align>
					<aligny>center</aligny>
					<label>$INFO[Container($PARAM[id]).ListItemPosition($PARAM[position]).Label]</label>
					<font>Keyboard_Button_5</font>
					<textcolor>$VAR[Unfocused_Color]</textcolor>
				</control>
			</control>
		</definition>
	</include>

	<include name="Numeric_Icon_Button">
		<param name="id" default="1000"/>
		<param name="position" default=""/>
		<param name="focusedtexture" default=""/>
		<param name="unfocusedtexture" default=""/>
		<definition>
			<control type="group">
				<width>120</width>
				<height>120</height>
				<control type="group">
					<visible>Control.HasFocus($PARAM[id]) + Container($PARAM[id]).Position($PARAM[position])</visible>
					<include content="$PARAM[focusedtexture]">
						<param name="color" value="$Var[Light_Color]"/>
					</include>
					<control type="image">
						<left>15</left>
						<right>15</right>
						<bottom>0</bottom>
						<height>10</height>
						<texture colordiffuse="$VAR[Accent_Color]">views/white_100.png</texture>
					</control>
				</control>
				<control type="group">
					<visible>![Control.HasFocus($PARAM[id]) + Container($PARAM[id]).Position($PARAM[position])]</visible>
					<include content="$PARAM[unfocusedtexture]">
						<param name="color" value="$VAR[Unfocused_Color]"/>
					</include>
				</control>
			</control>
		</definition>
	</include>

	<include name="Keyboard_Row_Layout">
		<param name="id" default=""/>
		<param name="visible" default="true"/>
		<param name="buttonwidth" default="120"/>
		<definition>
			<control type="grouplist">
				<width>100%</width>
				<height>120</height>
				<orientation>horizontal</orientation>
				<align>center</align>
				<visible>$PARAM[visible]</visible>
				<scrolltime tween="sine" easing="inout">360</scrolltime>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="-6"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="-5"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="-4"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="-3"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="-2"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="-1"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="0"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="1"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="2"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="3"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="4"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="5"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
				<include content="Keyboard_Label_Button">
					<param name="id" value="$PARAM[id]"/>
					<param name="position" value="6"/>
					<param name="width" value="$PARAM[buttonwidth]"/>
				</include>
			</control>
		</definition>
	</include>

	<include name="SmartPlaylistRule_List_Layout">
		<!-- Position0 -->
		<control type="image">
			<height>90</height>
			<texture colordiffuse="$VAR[Primary_Color]">views/white_100.png</texture>
		</control>
		<control type="label">
			<height>90</height>
			<left>15</left>
			<right>15</right>
			<aligny>center</aligny>
			<textcolor>$VAR[Secondary_Color]</textcolor>
			<font>List_Title_Focused</font>
			<label>$INFO[Container(1000).ListItem.Label]</label>
		</control>
		<!-- Positions 2 and 3 -->
		<control type="label">
			<height>90</height>
			<top>90</top>
			<left>15</left>
			<right>15</right>
			<aligny>center</aligny>
			<textcolor>$VAR[Light_Color]</textcolor>
			<font>List_Title_Unfocused</font>
			<label>$INFO[Container(1000).ListItem(1).Label]</label>
		</control>
		<control type="label">
			<height>90</height>
			<top>180</top>
			<left>15</left>
			<right>15</right>
			<aligny>center</aligny>
			<textcolor>$VAR[Light_Color]</textcolor>
			<font>List_Title_Unfocused</font>
			<label>$INFO[Container(1000).ListItem(2).Label]</label>
		</control>
	</include>

</includes>
