<includes>

	<!-- Furniture -->
	<include name="Transparent_Dialog_Panel">
		<param name="visible" default="true"/>
		<param name="left" default="60"/>
		<param name="right" default="60"/>
		<param name="top" default="240"/>
		<param name="bottom" default="240"/>
		<param name="colordiffuse" default="$VAR[Panel_Diffuse]"/>
		<definition>
			<control type="image">
				<visible>$PARAM[visible]</visible>
				<left>$PARAM[left]</left>
				<right>$PARAM[right]</right>
				<top>$PARAM[top]</top>
				<bottom>$PARAM[bottom]</bottom>
				<texture colordiffuse="$PARAM[colordiffuse]">views/white_100.png</texture>
			</control>
		</definition>
	</include>

	<include name="Transparent_Dialog_Background">
		<definition>
			<control type="image">
				<texture colordiffuse="$VAR[Transparent_Dialog_Mask]">views/white_100.png</texture>
			</control>
		</definition>
	</include>

	<include name="DialogDivider">
		<param name="visible" default="true"/>
		<param name="left" default="360"/>
		<param name="height" default="540"/>
		<definition>
			<control type="image">
				<visible>$PARAM[visible]</visible>
				<left>$PARAM[left]</left>
				<width>1</width>
				<centertop>50%</centertop>
				<height>$PARAM[height]</height>
				<aspectratio>scale</aspectratio>
				<texture colordiffuse="$VAR[Unfocused_Color]">views/white_100.png</texture>
			</control>
	</definition>
	</include>

	<include name="Dialog_Divider_With_Scrollbar">
		<param name="visible" default="true"/>
		<param name="dividervisible" default="true"/>
		<param name="scrollbarvisible" default="false"/>
		<param name="id" default="60"/>
		<param name="height" default="630"/>
		<param name="left" default="960"/>
		<param name="scrollbaralignleft" default="false"/>
		<param name="onleft" default="0"/>
		<param name="onright" default="0"/>
		<definition>
			<control type="group">
				<visible>$PARAM[visible]</visible>
				<left>$PARAM[left]</left>
				<centertop>50%</centertop>
				<height>$PARAM[height]</height>
				<control type="image">
					<visible>$PARAM[dividervisible]</visible>
					<width>1</width>
					<aspectratio>scale</aspectratio>
					<texture colordiffuse="$VAR[Unfocused_Color]">views/white_100.png</texture>
				</control>
				<control type="scrollbar" id="$PARAM[id]">
					<visible>$PARAM[scrollbarvisible]</visible>
					<animation effect="slide" end="-4" time="0" reversible="false" condition="$PARAM[scrollbaralignleft]">Conditional</animation>
					<left>$PARAM[scrollbarleft]</left>
					<width>5</width>
					<onleft>$PARAM[onleft]</onleft>
					<onright>$PARAM[onright]</onright>
					<centertop>50%</centertop>
					<height>$PARAM[height]</height>
					<showonepage>false</showonepage>
					<orientation>vertical</orientation>
					<texturesliderbackground></texturesliderbackground>
					<texturesliderbar colordiffuse="$VAR[Unfocused_Color]">views/white_100.png</texturesliderbar>
					<texturesliderbarfocus colordiffuse="$VAR[Light_Color]">views/white_100.png</texturesliderbarfocus>
					<textureslidernib></textureslidernib>
					<textureslidernibfocus></textureslidernibfocus>
				</control>
			</control>
	</definition>
	</include>

	<include name="DialogGrouplist">
		<width>1200</width>
		<centerleft>50%</centerleft>
		<centertop>50%</centertop>
		<height>542</height>
		<onright condition="Window.IsVisible(smartplaylistrule)">20</onright>
		<onright condition="!Window.IsVisible(smartplaylistrule)">9000</onright>
		<onleft>noop</onleft>
		<itemgap>0</itemgap>
		<align>center</align>
	</include>

	<!-- contextmenu -->
	<include name="ContextMenuLabelFocused">
		<definition>
			<height>60</height>
			<font>List_Title_Focused</font>
			<textcolor>$VAR[Light_Color]</textcolor>
		</definition>
	</include>

	<include name="ContextMenuLabelUnfocused">
		<definition>
			<height>55</height>
			<font>List_Title_Unfocused</font>
			<textcolor>$VAR[Light_Color]</textcolor>
		</definition>
	</include>

	<include name="Default_ContextMenu_Label">
		<control type="label">
			<include>ContextMenuLabelUnfocused</include>
			<label>$INFO[Control.GetLabel($PARAM[id])]</label>
			<visible>Control.IsVisible($PARAM[id]) + !Control.HasFocus($PARAM[id])</visible>
		</control>
		<control type="label">
			<include>ContextMenuLabelFocused</include>
			<label>$INFO[Control.GetLabel($PARAM[id])]</label>
			<visible>Control.IsVisible($PARAM[id]) + Control.HasFocus($PARAM[id])</visible>
		</control>
	</include>

	<include name="Settings_ContextMenu_Label">
		<control type="group">
			<height>90</height>
			<width>100%</width>
			<control type="label">
				<visible>Control.IsVisible($PARAM[id]) + !Control.HasFocus($PARAM[id])</visible>
				<font>Default_Settings_List</font>
				<textoffsetx>60</textoffsetx>
				<textcolor>$VAR[Unfocused_Color]</textcolor>
				<label>$INFO[Control.GetLabel($PARAM[id])]</label>
			</control>
			<control type="group">
				<visible>Control.IsVisible($PARAM[id]) + Control.HasFocus($PARAM[id])</visible>
				<control type="label">
					<font>Default_Settings_List</font>
					<textoffsetx>60</textoffsetx>
					<textcolor>$VAR[Light_Color]</textcolor>
					<label>$INFO[Control.GetLabel($PARAM[id])]</label>
				</control>
				<control type="image">
					<left>60</left>
					<right>0</right>
					<height>1</height>
					<bottom>0</bottom>
					<texture colordiffuse="$VAR[Unfocused_Color]">views/white_100.png</texture>
				</control>
			</control>
		</control>
	</include>


	<include name="ContextMenuLabelTemplate">
		<include condition="!Window.IsVisible(filemanager)" content="Default_ContextMenu_Label"/>
		<include condition="Window.IsVisible(filemanager)" content="Settings_ContextMenu_Label"/>
	</include>

	<!-- virtualkeyboard -->
	<include name="Keyboard_Label">
		<definition>
			<item>
				<description>$PARAM[description]</description>
				<visible>!String.IsEmpty(Control.GetLabel($PARAM[id]))</visible>
				<label>$INFO[Control.GetLabel($PARAM[id])]</label>
				<onclick>Sendclick($PARAM[id])</onclick>
