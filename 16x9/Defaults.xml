<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<default type="label">
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>0</textoffsetx>
		<textcolor>$VAR[Settings_Unfocused_Color]</textcolor>
		<disabledcolor>$VAR[Settings_Unfocused_Color]</disabledcolor>
		<selectedcolor>$VAR[Accent_Color]</selectedcolor>
	</default>

	<default type="textbox">
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>0</textoffsetx>
		<textcolor>$VAR[Primary_Color]</textcolor>
		<autoscroll delay="4000" time="4000" repeat="12000">true</autoscroll>
	</default>

	<default type="button">
		<width>1200</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>30</textoffsetx>
		<textcolor>$VAR[Settings_Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Primary_Color]</focusedcolor>
		<invalidcolor>$VAR[Settings_Unfocused_Color]</invalidcolor>
		<disabledcolor>$VAR[Settings_Unfocused_Color]</disabledcolor>
		<texturenofocus></texturenofocus>
		<texturefocus></texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="radiobutton">
		<width>1200</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>30</textoffsetx>
		<textcolor>$VAR[Settings_Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Primary_Color]</focusedcolor>
		<disabledcolor>$VAR[Settings_Unfocused_Color]</disabledcolor>
		<radiowidth>90</radiowidth>
		<radioheight>90</radioheight>
		<radioposx>1095</radioposx>
		<texturefocus></texturefocus>
		<textureradioonfocus colordiffuse="$VAR[Primary_Color]">dialogs/Radio_On_Focused.png</textureradioonfocus>
		<textureradioofffocus colordiffuse="$VAR[Primary_Color]">dialogs/Radio_Off_Focused.png</textureradioofffocus>
		<textureradioonnofocus colordiffuse="$VAR[Settings_Unfocused_Color]">dialogs/Radio_On_Unfocused.png</textureradioonnofocus>
		<textureradiooffnofocus colordiffuse="$VAR[Settings_Unfocused_Color]">dialogs/Radio_Off_Unfocused.png</textureradiooffnofocus>
		<textureradioondisabled colordiffuse="$VAR[Settings_Unfocused_Color]">dialogs/Radio_On_Unfocused.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="$VAR[Settings_Unfocused_Color]">dialogs/Radio_Off_Unfocused.png</textureradiooffdisabled>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrolex">
		<width>1200</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>30</textoffsetx>
		<textcolor>$VAR[Settings_Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Primary_Color]</focusedcolor>
		<disabledcolor>$VAR[Settings_Unfocused_Color]</disabledcolor>
		<spinwidth>60</spinwidth>
		<spinheight>90</spinheight>
		<texturefocus></texturefocus>
		<texturenofocus></texturenofocus>
		<textureup colordiffuse="$VAR[Settings_Unfocused_Color]">dialogs/spin_unfocused.png</textureup>
		<textureupdisabled colordiffuse="$VAR[Settings_Unfocused_Color]">dialogs/spin_up.png</textureupdisabled>
		<textureupfocus colordiffuse="$VAR[Primary_Color]">dialogs/spin_focused.png</textureupfocus>
		<texturedown colordiffuse="$VAR[Settings_Unfocused_Color]" flipy="true">dialogs/spin_unfocused.png</texturedown>
		<texturedowndisabled colordiffuse="$VAR[Settings_Unfocused_Color" flipy="true">dialogs/spin_unfocused.png</texturedowndisabled>
		<texturedownfocus colordiffuse="$VAR[Primary_Color]" flipy="true">dialogs/spin_focused.png</texturedownfocus>
		<reverse>no</reverse>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="sliderex">
		<width>1200</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>30</textoffsetx>
		<textcolor>$VAR[Settings_Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Primary_Color]</focusedcolor>
		<disabledcolor>$VAR[Settings_Unfocused_Color]</disabledcolor>
		<texturefocus></texturefocus>
		<texturenofocus></texturenofocus>
		<sliderwidth>120</sliderwidth>
		<sliderheight>30</sliderheight>
		<orientation>vertical</orientation>
		<texturesliderbar colordiffuse="$VAR[Settings_Unfocused_Color]">dialogs/slider_background.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[Settings_Unfocused_Color]">dialogs/slider_nib_unfocused.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[Primary_Color]">dialogs/slider_nib_focused.png</textureslidernibfocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="edit">
		<width>1200</width>
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Settings_List</font>
		<textoffsetx>30</textoffsetx>
		<textcolor>$VAR[Settings_Unfocused_Color]</textcolor>
		<focusedcolor>$VAR[Dark_Color]</focusedcolor>
		<disabledcolor>$VAR[Settings_Unfocused_Color]</disabledcolor>
		<invalidcolor>$VAR[Settings_Unfocused_Color]</invalidcolor>
		<scroll>true</scroll>
		<scrollsuffix> / </scrollsuffix>
		<hinttext/>
		<texturefocus colordiffuse="$VAR[Primary_Color]">views/white_100.png</texturefocus>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="progress">
		<width></width>
		<height></height>
		<texturebg></texturebg>
		<lefttexture></lefttexture>
		<midtexture></midtexture>
		<righttexture></righttexture>
		<overlaytexture></overlaytexture>
	</default>

	<default type="togglebutton">
		<height>60</height>
		<width>60</width>
		<align></align>
		<aligny></aligny>
		<label></label>
		<font></font>
		<textoffsetx></textoffsetx>
		<textcolor></textcolor>
		<focusedcolor></focusedcolor>
		<disabledcolor></disabledcolor>
		<texturefocus colordiffuse="$VAR[Primary_Color]"></texturefocus>
		<texturenofocus colordiffuse="$VAR[Primary_Color]"></texturenofocus>
		<alttexturefocus colordiffuse="$VAR[Primary_Color]"></alttexturefocus>
		<alttexturenofocus colordiffuse="$VAR[Primary_Color]"></alttexturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>

</includes>
