<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<default type="label">
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>List_Title_Unfocused</font>
		<textcolor>$VAR[PrimaryColor]</textcolor>
		<disabledcolor>$VAR[MediumGreyColor]</disabledcolor>
		<selectedcolor>$VAR[AccentColor]</selectedcolor>
	</default>

	<default type="button">
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Button</font>
		<textoffsetx>15</textoffsetx>
		<textcolor>$VAR[LightColor]</textcolor>
		<focusedcolor>$VAR[SecondaryColor]</focusedcolor>
		<invalidcolor>$VAR[MediumGreyColor]</invalidcolor>
		<disabledcolor>$VAR[MediumGreyColor]</disabledcolor>
		<texturenofocus></texturenofocus>
		<texturefocus colordiffuse="$VAR[PrimaryColor]">views/white_100.png</texturefocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="radiobutton">
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Button</font>
		<textoffsetx>15</textoffsetx>
		<textcolor>$VAR[LightColor]</textcolor>
		<focusedcolor>$VAR[SecondaryColor]</focusedcolor>
		<disabledcolor>$VAR[MediumGreyColor]</disabledcolor>
		<radiowidth>90</radiowidth>
		<radioheight>90</radioheight>
		<texturefocus colordiffuse="$VAR[PrimaryColor]">views/white_100.png</texturefocus>
		<textureradioonfocus colordiffuse="$VAR[SecondaryColor]">osd/Radio_On_Focused.png</textureradioonfocus>
		<textureradioofffocus colordiffuse="$VAR[SecondaryColor]">osd/Radio_Off_Focused.png</textureradioofffocus>
		<textureradioonnofocus colordiffuse="$VAR[LightColor]">osd/Radio_On_Unfocused.png</textureradioonnofocus>
		<textureradiooffnofocus colordiffuse="$VAR[LightColor]">osd/Radio_Off_Unfocused.png</textureradiooffnofocus>
		<textureradioondisabled colordiffuse="$VAR[MediumGreyColor]">osd/Radio_On_Unfocused.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="$VAR[MediumGreyColor]">osd/Radio_Off_Unfocused.png</textureradiooffdisabled>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrolex">
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>List_Title_Unfocused</font>
		<textoffsetx>15</textoffsetx>
		<textcolor>$VAR[LightColor]</textcolor>
		<focusedcolor>$VAR[SecondaryColor]</focusedcolor>
		<disabledcolor>$VAR[MediumGreyColor]</disabledcolor>
		<spinwidth>55</spinwidth>
		<spinheight>75</spinheight>
		<texturefocus colordiffuse="$VAR[PrimaryColor]">views/white_100.png</texturefocus>
		<texturenofocus></texturenofocus>
		<textureup colordiffuse="$VAR[PrimaryColor]">dialogs/spin_up.png</textureup>
		<texturedown colordiffuse="$VAR[PrimaryColor]">dialogs/spin_down.png</texturedown>
		<textureupfocus colordiffuse="$VAR[PrimaryColor]">dialogs/spin_up.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[TexPrimaryColortColor]">dialogs/spin_down.png</texturedownfocus>
		<textureupdisabled colordiffuse="$VAR[PrimaryColor]">dialogs/spin_up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="$VAR[PrimaryColor]">dialogs/spin_down.png</texturedowndisabled>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="edit">
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Default_Button</font>
		<textoffsetx>15</textoffsetx>
		<textcolor>$VAR[LightColor]</textcolor>
		<focusedcolor>$VAR[SecondaryColor]</focusedcolor>
		<disabledcolor>$VAR[MediumGreyColor]</disabledcolor>
		<invalidcolor>$VAR[MediumGreyColor]</invalidcolor>
		<scroll>true</scroll>
		<scrollsuffix> / </scrollsuffix>
		<hinttext/>
		<texturefocus colordiffuse="$VAR[PrimaryColor]">views/white_100.png</texturefocus>
		<pulseonselect>no</pulseonselect>
	</default>

	<default type="sliderex">
		<height>90</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>List_Title_Unfocused</font>
		<textcolor>$VAR[LightColor]</textcolor>
		<focusedcolor>$VAR[SecondaryColor]</focusedcolor>
		<disabledcolor>$VAR[MediumGreyColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[PrimaryColor]">views/white_100.png</texturefocus>
		<texturenofocus></texturenofocus>
		<sliderwidth>200</sliderwidth>
		<sliderheight>20</sliderheight>
		<texturesliderbar colordiffuse="$VAR[PrimaryColor]">common/white.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[PrimaryColor]">common/white.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[PrimaryColor]">common/white.png</textureslidernibfocus>
		<textoffsetx>15</textoffsetx>
		<pulseonselect>false</pulseonselect>
	</default>







	<default type="progress">
		<width></width>
		<height></height>
		<texturebg></texturebg>
		<lefttexture></lefttexture>
		<midtexture></midtexture>
		<righttexture></righttexture>
		<overlaytexture></overlaytexture>
	</default>
	<default type="fadelabel">
		<height>78</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>List_Title_Focused</font>
		<textcolor>$VAR[PrimaryColor]</textcolor>
		<scrollout>false</scrollout>
		<scrollspeed>40</scrollspeed>
		<pauseatend>2000</pauseatend>
	</default>


	<default type="togglebutton">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<label></label>
		<textoffsetx>10</textoffsetx>
		<font>List_Title_Unfocused</font>
		<textcolor>$VAR[PrimaryColor]</textcolor>
		<focusedcolor>$VAR[PrimaryColor]</focusedcolor>
		<disabledcolor>$VAR[PrimaryColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[PrimaryColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[PrimaryColor]">views/white_10.png</texturenofocus>
		<alttexturefocus colordiffuse="$VAR[PrimaryColor]">views/white_100.png</alttexturefocus>
		<alttexturenofocus colordiffuse="$VAR[PrimaryColor]">views/white_10.png</alttexturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrol">
		<height>75</height>
		<width>75</width>
		<align>left</align>
		<aligny>center</aligny>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<font>List_Title_Unfocused</font>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[PrimaryColor]</textcolor>
		<focusedcolor>$VAR[PrimaryColor]</focusedcolor>
		<disabledcolor>$VAR[PrimaryColor]</disabledcolor>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<textureup colordiffuse="$VAR[PrimaryColor]">dialogs/spin_up.png</textureup>
		<texturedown colordiffuse="$VAR[PrimaryColor]">dialogs/spin_down.png</texturedown>
		<textureupfocus colordiffuse="$VAR[PrimaryColor]">dialogs/spin_up.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[PrimaryColor]">dialogs/spin_down.png</texturedownfocus>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="slider">
		<height>23</height>
		<orientation>horizontal</orientation>
		<texturefocus colordiffuse="$VAR[PrimaryColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[PrimaryColor]">views/white_10.png</texturenofocus>
	</default>
	<default type="textbox">
		<font>List_Title_Unfocused</font>
		<textcolor>$VAR[PrimaryColor]</textcolor>
		<autoscroll delay="8000" time="4000" repeat="12000">Skin.HasSetting(ScrollPlot)</autoscroll>
	</default>

</includes>
