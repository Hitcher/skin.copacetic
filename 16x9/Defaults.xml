<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<default type="progress">
		<width></width>
		<height></height>
		<texturebg></texturebg>
		<lefttexture></lefttexture>
		<midtexture></midtexture>
		<righttexture></righttexture>
		<overlaytexture></overlaytexture>
	</default>

	<default type="button">
		<height></height>
		<align></align>
		<aligny></aligny>
		<font></font>
		<label></label>
		<textoffsetx></textoffsetx>
		<textcolor></textcolor>
		<focusedcolor></focusedcolor>
		<invalidcolor></invalidcolor>
		<disabledcolor></disabledcolor>
		<texturefocus></texturefocus>
		<texturenofocus></texturenofocus>
		<pulseonselect></pulseonselect>
	</default>


	<default type="label">
		<height></height>
		<align></align>
		<aligny></aligny>
		<font></font>
		<textcolor></textcolor>
		<disabledcolor></disabledcolor>
		<selectedcolor></selectedcolor>
	</default>






	<default type="fadelabel">
		<height>78</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>ListFocused</font>
		<textcolor>$VAR[PrimaryColor]</textcolor>
		<scrollout>false</scrollout>
		<scrollspeed>40</scrollspeed>
		<pauseatend>2000</pauseatend>
	</default>




	<default type="togglebutton">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<label></label>
		<textoffsetx>10</textoffsetx>
		<font>Light36</font>
		<textcolor>$VAR[TextColor]</textcolor>
		<focusedcolor>$VAR[TextColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[BackgroundColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<alttexturefocus colordiffuse="$VAR[BackgroundColor]">views/white_100.png</alttexturefocus>
		<alttexturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</alttexturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="radiobutton">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Light36</font>
		<label></label>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[TextColor]</textcolor>
		<focusedcolor>$VAR[TextColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<radiowidth />
		<radioheight />
		<texturefocus colordiffuse="$VAR[BackgroundColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<textureradioonfocus/>
		<textureradioofffocus/>
		<textureradioonnofocus/>
		<textureradiooffnofocus/>
		<textureradioondisabled/>
		<textureradiooffdisabled/>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrolex">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<label></label>
		<font>Light36</font>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[TextColor]</textcolor>
		<focusedcolor>$VAR[TextColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<spinwidth>55</spinwidth>
		<spinheight>75</spinheight>
		<texturefocus colordiffuse="$VAR[BackgroundColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<textureup colordiffuse="$VAR[UnFocusedColor]">dialogs/spin_up.png</textureup>
		<texturedown colordiffuse="$VAR[UnFocusedColor]">dialogs/spin_down.png</texturedown>
		<textureupfocus colordiffuse="$VAR[TextColor]">dialogs/spin_up.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[TextColor]">dialogs/spin_down.png</texturedownfocus>
		<textureupdisabled colordiffuse="$VAR[DisabledColor]">dialogs/spin_up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="$VAR[DisabledColor]">dialogs/spin_down.png</texturedowndisabled>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrol">
		<height>75</height>
		<width>75</width>
		<align>left</align>
		<aligny>center</aligny>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<font>Light36</font>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[TextColor]</textcolor>
		<focusedcolor>$VAR[TextColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<textureup colordiffuse="$VAR[UnFocusedColor]">dialogs/spin_up.png</textureup>
		<texturedown colordiffuse="$VAR[UnFocusedColor]">dialogs/spin_down.png</texturedown>
		<textureupfocus colordiffuse="$VAR[TextColor]">dialogs/spin_up.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[TextColor]">dialogs/spin_down.png</texturedownfocus>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="sliderex">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Light36</font>
		<textcolor>$VAR[TextColor]</textcolor>
		<focusedcolor>$VAR[TextColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[BackgroundColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<sliderwidth>200</sliderwidth>
		<sliderheight>20</sliderheight>
		<texturesliderbar colordiffuse="$VAR[DisabledColor]">common/white.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[UnFocusedColor]">common/white.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[TextColor]">common/white.png</textureslidernibfocus>
		<textoffsetx>10</textoffsetx>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="slider">
		<height>23</height>
		<orientation>horizontal</orientation>
		<texturefocus colordiffuse="$VAR[BackgroundColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
	</default>
	<default type="textbox">
		<font>Font27</font>
		<textcolor>$VAR[TextColor]</textcolor>
		<autoscroll delay="8000" time="4000" repeat="12000">Skin.HasSetting(ScrollPlot)</autoscroll>
	</default>
	<default type="edit">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Light36</font>
		<hinttext/>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[TextColor]</textcolor>
		<focusedcolor>$VAR[BackgroundColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<invalidcolor>$VAR[DisabledColor]</invalidcolor>
		<texturefocus colordiffuse="$VAR[TextColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[BackgroundColor]">views/white_100.png</texturenofocus>
		<pulseonselect>no</pulseonselect>
	</default>
</includes>
