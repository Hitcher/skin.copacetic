<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<default type="label">
		<height>36</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<selectedcolor>$VAR[SelectedColor]</selectedcolor>
		<scroll>false</scroll>
	</default>

	<default type="fadelabel">
		<height>36</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<scrollout>false</scrollout>
		<scrollspeed>40</scrollspeed>
		<pauseatend>2000</pauseatend>
	</default>

	<default type="button">
		<width>400</width>
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<label>-</label>
		<textoffsetx>0</textoffsetx>
		<textcolor>$VAR[UnFocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<invalidcolor>$VAR[DisabledColor]</invalidcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus>-</texturefocus>
		<texturenofocus>-</texturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="togglebutton">
		<width>400</width>
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<label>-</label>
		<textoffsetx>0</textoffsetx>
		<font>Font36</font>
		<textcolor>$VAR[UnFocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturenofocus>-</texturenofocus>
		<texturefocus>-</texturefocus>
		<alttexturenofocus>-</alttexturenofocus>
		<alttexturefocus>-</alttexturefocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="radiobutton">
		<width>400</width>
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<label>-</label>
		<textcolor>$VAR[UnFocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<radiowidth>16</radiowidth>
		<radioheight>16</radioheight>
		<texturenofocus>-</texturenofocus>
		<texturefocus>-</texturefocus>
		<textureradioonfocus>common/RadioOn.png</textureradioonfocus>
		<textureradioonnofocus>common/RadioOn.png</textureradioonnofocus>
		<textureradioofffocus>common/RadioOff.png</textureradioofffocus>
		<textureradiooffnofocus>common/RadioOff.png</textureradiooffnofocus>
		<textoffsetx>0</textoffsetx>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrolex">
		<width>400</width>
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<label>-</label>
		<font>Font36</font>
		<spinwidth>35</spinwidth>
		<spinheight>66</spinheight>
		<texturenofocus>-</texturenofocus>
		<texturefocus>-</texturefocus>
		<textureup>common/ArrowUpNF.png</textureup>
		<texturedown>common/ArrowDownNF.png</texturedown>
		<textureupfocus>common/ArrowUpFO.png</textureupfocus>
		<texturedownfocus>common/ArrowDownFO.png</texturedownfocus>
		<textcolor>$VAR[UnFocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<textoffsetx>0</textoffsetx>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrol">
		<height>66</height>
		<width>66</width>
		<align>left</align>
		<aligny>center</aligny>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<textureup>common/ArrowUpNF.png</textureup>
		<texturedown>common/ArrowDownNF.png</texturedown>
		<textureupfocus>common/ArrowUpFO.png</textureupfocus>
		<texturedownfocus>common/ArrowDownFO.png</texturedownfocus>
		<font>Font36</font>
		<textcolor>$VAR[UnFocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<textoffsetx>0</textoffsetx>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="sliderex">
		<height>66</height>
		<width>183</width>
		<align>left</align>
		<aligny>center</aligny>
		<texturenofocus>-</texturenofocus>
		<texturefocus>-</texturefocus>
		<sliderwidth>230</sliderwidth>
		<sliderheight>23</sliderheight>
		<font>Font36</font>
		<textcolor>$VAR[UnFocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<textoffsetx>0</textoffsetx>
	</default>

	<default type="slider">
		<width>360</width>
		<height>23</height>
		<texturefocus>-</texturefocus>
		<texturenofocus>-</texturenofocus>
	</default>

	<default type="progress">
		<height>44</height>
		<texturebg>-</texturebg>
		<lefttexture>-</lefttexture>
		<righttexture>-</righttexture>
		<overlaytexture>-</overlaytexture>
	</default>

	<default type="textbox">
		<font>Font27</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<autoscroll delay="8000" time="4000" repeat="12000">Skin.HasSetting(ScrollPlot)</autoscroll>
	</default>

	<default type="edit">
		<height>66</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<textoffsetx>0</textoffsetx>
		<textcolor>$VAR[UnFocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<invalidcolor>$VAR[DisabledColor]</invalidcolor>
		<texturefocus>-</texturefocus>
		<pulseonselect>no</pulseonselect>
	</default>

</includes>