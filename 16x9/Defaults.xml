<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="label">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<selectedcolor>$VAR[SelectedColor]</selectedcolor>
		<scroll>false</scroll>
	</default>
	<default type="fadelabel">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<scrollout>false</scrollout>
		<scrollspeed>40</scrollspeed>
		<pauseatend>2000</pauseatend>
	</default>
	<default type="button">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<label>-</label>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<invalidcolor>$VAR[DisabledColor]</invalidcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="togglebutton">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<label>-</label>
		<textoffsetx>10</textoffsetx>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<alttexturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</alttexturefocus>
		<alttexturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</alttexturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="radiobutton">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<label>-</label>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<radiowidth />
		<radioheight />
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<textureradioonfocus/>
		<textureradioofffocus/>
		<textureradioonnofocus/>
		<textureradiooffnofocus/>
		<textureradioondisabled/>
		<textureradiooffdisabled/>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrolex">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<label>-</label>
		<font>Font36</font>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<spinwidth>55</spinwidth>
		<spinheight>75</spinheight>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<textureup colordiffuse="$VAR[UnFocusedColor]">dialogs/spin_up.png</textureup>
		<texturedown colordiffuse="$VAR[UnFocusedColor]">dialogs/spin_down.png</texturedown>
		<textureupfocus colordiffuse="$VAR[FocusedColor]">dialogs/spin_up.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[FocusedColor]">dialogs/spin_down.png</texturedownfocus>
		<textureupdisabled colordiffuse="$VAR[DisabledColor]">dialogs/spin_up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="$VAR[DisabledColor]">dialogs/spin_down.png</texturedowndisabled>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="spincontrol">
		<height>75</height>
		<width>75</width>
		<align>left</align>
		<aligny>center</aligny>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<font>Font36</font>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<textureup colordiffuse="$VAR[UnFocusedColor]">dialogs/spin_up.png</textureup>
		<texturedown colordiffuse="$VAR[UnFocusedColor]">dialogs/spin_down.png</texturedown>
		<textureupfocus colordiffuse="$VAR[FocusedColor]">dialogs/spin_up.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[FocusedColor]">dialogs/spin_down.png</texturedownfocus>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="sliderex">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
		<sliderwidth>200</sliderwidth>
		<sliderheight>20</sliderheight>
		<texturesliderbar colordiffuse="$VAR[DisabledColor]">common/white.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[UnFocusedColor]">common/white.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[FocusedColor]">common/white.png</textureslidernibfocus>
		<textoffsetx>10</textoffsetx>
		<pulseonselect>false</pulseonselect>
	</default>
	<default type="slider">
		<height>23</height>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[DisabledColor]">views/white_10.png</texturenofocus>
	</default>
	<default type="progress">
		<height>44</height>
		<texturebg>-</texturebg>
		<lefttexture>-</lefttexture>
		<righttexture>-</righttexture>
		<overlaytexture>-</overlaytexture>
	</default>
	<default type="textbox">
		<font>Font27</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<autoscroll delay="8000" time="4000" repeat="12000">Skin.HasSetting(ScrollPlot)</autoscroll>
	</default>
	<default type="edit">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<hinttext/>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[ButtonColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<invalidcolor>$VAR[DisabledColor]</invalidcolor>
		<texturefocus colordiffuse="$VAR[FocusedColor]">views/white_100.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturenofocus>
		<pulseonselect>no</pulseonselect>
	</default>
</includes>